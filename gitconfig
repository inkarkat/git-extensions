	yearrbni = revision-command --keep-position year-command --revision REVISION --no-range --one-more -2 rbni TIMESPAN
	yearmove-to-branch = revision-command --keep-position year-command --revision REVISION --no-range --one-more +1 TIMESPAN uncommit-to-branch --exclude-commit
