	d-ytoselectedq = d-ytoselected --max-count 25
	d-ytoselectedz = d-ytoselected --max-count 100
	d-ytoselectedcommitref = selectedref-command --commits-only --single-only -2 diffY COMMITS
	d-ytolastcommitref = lastcommitref-command -2 diffY COMMITS

	drtoselected = selectedcommit-command --single-only -2 dr COMMITS
	drtoselected1 = drtoselected --max-count 1
	drtoselected2 = drtoselected --max-count 2
	drtoselected3 = drtoselected --max-count 3
	drtoselected4 = drtoselected --max-count 4
	drtoselected5 = drtoselected --max-count 5
	drtoselected6 = drtoselected --max-count 6
	drtoselected7 = drtoselected --max-count 7
