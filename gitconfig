	lhteam6 = lhteam --max-count 6
	lhteam7 = lhteam --max-count 7
	lhby = dashdash-default-command : others-command -2 lh AUTHORS : ; git lh, filtered for that user(s)' commits.
	lhby1 = dashdash-default-command : others-command -4 lh --max-count 1 AUTHORS :
	lhby2 = dashdash-default-command : others-command -4 lh --max-count 2 AUTHORS :
	lhby3 = dashdash-default-command : others-command -4 lh --max-count 3 AUTHORS :
	lhby4 = dashdash-default-command : others-command -4 lh --max-count 4 AUTHORS :
	lhby5 = dashdash-default-command : others-command -4 lh --max-count 5 AUTHORS :
	lhby6 = dashdash-default-command : others-command -4 lh --max-count 6 AUTHORS :
	lhby7 = dashdash-default-command : others-command -4 lh --max-count 7 AUTHORS :
	lhlastby = dashdash-default-command : others-command -3 byuserrange-command lh AUTHORS :	; git lh of the last consecutive commit(s) done by the passed or queried user(s)

	;; lurl: URLs to passed <remote> of all found commits.
	lourl = lurl origin	; My GitHub clone URLs of all found commits.
	lourl1 = lourl --max-count 1
	lourl2 = lourl --max-count 2
	lourl3 = lourl --max-count 3
	lourl4 = lourl --max-count 4
	lourl5 = lourl --max-count 5
	lourl6 = lourl --max-count 6
	lourl7 = lourl --max-count 7
