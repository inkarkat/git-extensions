	lgbyq = dashdash-default-command : others-command -4 lg --max-count 25 AUTHORS :
	lgbyz = dashdash-default-command : others-command -4 lg --max-count 100 AUTHORS :
	lglastby = dashdash-default-command : others-command -3 byuserrange-command lg AUTHORS :	; git lg of the last consecutive commit(s) done by the passed or queried user(s)

