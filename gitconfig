	b2lfixuprb = rev-range --revision @{-2} --end-revision HEAD -2 fixupselectedrb RANGE
	b2lcommitidentical = branch-command --keep-position rev-range --revision @{-2} --end-revision BRANCH -2 commitidenticalselected RANGE
	b2lcommitidenticalg = branch-command --keep-position rev-range --revision @{-2} --end-revision BRANCH -2 commitidenticalselectedg RANGE
