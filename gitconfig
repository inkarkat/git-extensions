	lgby1 = dashdash-default-command : others-command -4 lg --max-count 1 AUTHORS :
	lgby2 = dashdash-default-command : others-command -4 lg --max-count 2 AUTHORS :
	lgby3 = dashdash-default-command : others-command -4 lg --max-count 3 AUTHORS :
