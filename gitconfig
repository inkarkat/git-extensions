	ubmlogmine = rbrrefdo-wrapper upstream master-command --keep-position rev-range --revision MASTER --end-revision {} -2 logmine RANGE
	ubmlogteam = rbrrefdo-wrapper upstream master-command --keep-position rev-range --revision MASTER --end-revision {} -2 logteam RANGE
	ubmlogby = rbrrefdo-wrapper upstream dashdash-default-command : master-command --keep-position rev-range --revision MASTER --end-revision {} -5 others-command -2 log AUTHORS RANGE :
	; ubmrevert does not make sense with branch references
	; ubmrevertcommit does not make sense with branch references
	; ubmcorrect does not make sense with branch references
	; ubmfixup does not make sense with branch references
	; ubmfixuprb does not make sense with branch references
	; ubmcommitidentical does not make sense with branch references
	; ubmcommitlike does not make sense with branch references
	; ubmcommitrelate does not make sense with branch references
	; ubmrbi does not make sense with branch references
	; ubmrbni does not make sense with branch references
	; ubmwipe does not make sense
	; ubmbase cannot be (easily) passed branch references.
	; ubmbaselg cannot be (easily) passed branch references.
	; ubmpred cannot be (easily) passed branch references.
	; ubmpredlg cannot be (easily) passed branch references.
	ubmwhocreatedthosechangedfiles = rbrrefdo-wrapper upstream --no-git-color files-command --source-command brfiles whocreated
	ubmwhocreatedhere = rbrrefdo-wrapper upstream --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -2 whocreated RANGE
	ubmwhodidthosechangedfiles = rbrrefdo-wrapper upstream --no-git-color files-command --source-command brfiles whodid
	ubmwhodidhere = rbrrefdo-wrapper upstream --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -2 whodid RANGE
	ubmwhoownsthosechangedfiles = rbrrefdo-wrapper upstream --no-git-color files-command --source-command brfiles whoowns
	; ubmwhoownsthechanges even with the changed files would not show useful data
	ubmwhocontributedthosechangedfiles = rbrrefdo-wrapper upstream --no-git-color files-command --source-command brfiles whocontributed
	ubmwhocontributedhere = rbrrefdo-wrapper upstream --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -2 whocontributed RANGE
	ubmwhowhen = rbrrefdo-wrapper upstream --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -2 whowhen RANGE
	ubmwhofirst = rbrrefdo-wrapper upstream --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -2 whofirst RANGE
	ubmwholast = rbrrefdo-wrapper upstream --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -2 wholast RANGE
	ubmwhatdid = rbrrefdo-wrapper upstream --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -2 whatdid RANGE
	ubmchurn = rbrrefdo-wrapper upstream --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -2 churn RANGE
