	ssc-i5 = ssc-i --max-count 5
	ssc-i6 = ssc-i --max-count 6
	ssc-i7 = ssc-i --max-count 7

	ssa = showaselected
	ssa1 = showaselected --max-count 1
	ssa2 = showaselected --max-count 2
	ssa3 = showaselected --max-count 3
	ssa4 = showaselected --max-count 4
	ssa5 = showaselected --max-count 5
	ssa6 = showaselected --max-count 6
	ssa7 = showaselected --max-count 7
	ssag = selectedgcommit-command --single-only -2 showa COMMITS
	ssast = stfiles-command showaselected
	ssalastst = stfiles-command selectedcommit-command --accept-single --single-only -2 showa COMMITS --max-count 1
	ssast1 = ssast --max-count 1
