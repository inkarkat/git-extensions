	drtolastcommitref = lastcommitref-command -2 dr COMMITS

	drltoselected = selectedcommit-command --single-only -2 drl COMMITS
	drltoselected1 = drltoselected --max-count 1
	drltoselected2 = drltoselected --max-count 2
	drltoselected3 = drltoselected --max-count 3
	drltoselected4 = drltoselected --max-count 4
	drltoselected5 = drltoselected --max-count 5
	drltoselected6 = drltoselected --max-count 6
	drltoselected7 = drltoselected --max-count 7
	drltoselectedx = drltoselected --max-count 12
	drltoselectedq = drltoselected --max-count 25
	drltoselectedz = drltoselected --max-count 100
	drltoselectedcommitref = selectedref-command --commits-only --single-only -2 drl COMMITS
	drltolastcommitref = lastcommitref-command -2 drl COMMITS

	; Simple patch creation:
	;	$ git diff 1b6d > my.patch
	; On the receiving end, apply via:
	; 	$ git apply my.patch
	;; applycheck: Try to apply the passed patch and print statistics and result.
	;; applyrcheck: Try to apply the passed patch to the root and print statistics and result.
	;; applyr: Apply the patch from the root (as most patches should be based on the toplevel repository directory).
	;
	; Convert commit(s) to a (set of) patches [ <since> | <revision range> ],
	; ready to send by email, by default saved to CWD / -o outdir in mbox
	; format:
	; 	$ git format-patch --stdout 1b6d..HEAD^^
	; which can be passed to git send-email.
	ma = format-patch
	; On the receiving end, save an email to a file, then type:
	; 	$ git am < email.txt
	; Extract commits between revisions R1 and R2, and apply them on top of the current branch using git am to cherry-pick them:
	;   $ git ma -k --stdout R1..R2 | git am -3 -k
	; This applies the incoming patch and also creates a commit, including information such as the author.
	applymbox = am

	g = grep --color -n
	gf = grep --files-with-matches
	g-f = grep --files-without-match

	;; ls: List files under version control; skip unversioned files.
	lsr = ls --recursive	; List the whole hierarchy of files under version control.
	lsrr = ls --recursive --submodule recurse	; List the whole hierarchy of files under version control, including submodules.
	ll = ls --long --relative-date	; List hash, author, and date of the last commit for each file under source control.
	llr = ls --long --relative-date --recursive	; List hash, author, and date of the last commit for the whole hierarchy under source control.
	llrr = ls --long --relative-date --recursive --submodule recurse	; List hash, author, and date of the last commit for the whole hierarchy including submodules.
	;; lll: List all files under source control with relative paths from the working copy's root.
	;; lllbrurl: List URLs to passed <remote> of all files under source control in the current branch. In contrast to lllhurl, this link will always point to the latest version on that branch.
	lllbrourl = lllbrurl origin
	lllbruurl = lllbrurl upstream
	lllmourl = master-command --keep-position lllbrurl origin --branch MASTER	; List URLs to my GitHub clone of all files under source control in the master branch. In contrast to lllhourl, this link will always point to the latest version on that branch.
	lllmuurl = master-command --keep-position lllbrurl upstream --branch MASTER	; List URLs to upstream of all files under source control in the master branch. In contrast to lllhuurl, this link will always point to the latest version on that branch.
	lllnourl = base-command --keep-position lllbrurl origin --branch BASE	; List URLs to my GitHub clone of all files under source control in the base branch. In contrast to lllhourl, this link will always point to the latest version on that branch.
	lllnuurl = base-command --keep-position lllbrurl upstream --branch BASE	; List URLs to upstream of all files under source control in the base branch. In contrast to lllhuurl, this link will always point to the latest version on that branch.
	;; lllhurl: List URLs to passed <remote> of all files under source control with the current / passed revision. In contrast to lllbrurl (which incorporates future changes done in the branch), this creates a permalink to the current file's contents.
	lllhourl = lllhurl origin
	lllhuurl = lllhurl upstream

	;; eachFile: Execute COMMAND on each FILE under version control (which either is appended or replaces any {} marker inside COMMANDLINE) individually; unversioned files are skipped.
	;; eachDir: Execute COMMAND on each DIR under version control (which either is appended or replaces any {} marker inside COMMANDLINE) individually; unversioned directories are skipped.

	;; datediff: Show the time range of the commits done in the passed <revision-range>, or --since=<date>.

	;; ident-update: Update the expansion of $Id:$ via ident in the .gitattributes after a commit. Can --install a hook to do this automatically on each commit.
	; How to enable what strings:
	; 	1. echo '@(#) file $Id$ project' >> file
	; 	2. echo '* ident' >> .gitattributes
	;   3. git commit file .gitattributes
	;; find-blob: Find commit(s) (the last one is the earliest) which have the passed hash of a blob in their tree. Use to resolve the $Id$ put into a file via the "ident" gitattribute, e.g. in a what string.
	;; hash-object: Print the hash of the passed file (which must not have the $Id$ expanded!)
	; This is the same as
	;   (echo -e -n "blob $(stat -c %s "$file")\0"; cat "$file") | sha1sum

	;; log: Show commit logs, decorate with ref names, show in topological (instead of chronological) order, keeping merged branches together.
	log1 = log --decorate --max-count 1
	log2 = log --decorate --max-count 2
	log3 = log --decorate --max-count 3
	log4 = log --decorate --max-count 4
	log5 = log --decorate --max-count 5
	log6 = log --decorate --max-count 6
	log7 = log --decorate --max-count 7
	logx = log --decorate --max-count 12
	logq = log --decorate --max-count 25
	logz = log --decorate --max-count 100

	;; rnlog: Show commit logs, limiting the number counting from the beginning instead of from the end. (You cannot do this via git log --reverse --max-count, because commit limiting is done before commit ordering.)
	;; rnlg: One-line topology, author, date, tags and commit summary, limiting the number counting from the beginning instead of from the end.

	logtimeshifted = discrepancylog --field1 AuthorDate: --field2 CommitDate: --pretty=fuller ; Show logs of amended or rebased commits only.
	logonbehalf = discrepancylog --field1 Author: --field2 Commit: --pretty=fuller	; Show logs where author and committer are not the same person.


	;; logg: Log only those commits where log messages match the specified text or /regexp/.
	loggmine = mine-command -2 logg ME
	;; loggv
	loggvmine = mine-command -2 loggv ME
	;; lgg
	lggmine = mine-command -2 lgg ME
	;; lcg
	lcgmine = mine-command -2 lcg ME
	;; lclg
	lclgmine = mine-command -2 lclg ME
	;; loggfiles
	loggversions = findcontained logg annotated
	loggtags = findcontained logg describe

	changedlog = changed-command log	; changedlog does not look at the log message at all, but the actual diff; i.e. those filepairs whose preimage and postimage have different number of occurrences of the text or /regexp/.
	changedlogmine = mine-command -2 changedlog ME
	changedlogv = changed-command logv
	changedlogvmine = mine-command -2 changedlogv ME
	changedlg = changed-command lg
	changedlgmine = mine-command -2 changedlg ME
	changedlc = changed-command lc
	changedlcmine = mine-command -2 changedlc ME
	changedlcl = changed-command lcl
	changedlclmine = mine-command -2 changedlcl ME
	changedfiles = changed-command logfilenames --unique
	changedversions = findcontained changedlog annotated
	changedtags = findcontained changedlog describe

	touchedlog = changedlog --more	; touchedlog is a more encompassing variant of changedlog that not just shows those commits where the number of occurrences of text or /regexp/ changes, but also lines matching text or /regexp/ that just were somehow modified, without changing the number of occurrences themselves.
	touchedlogmine = changedlogmine --more
	touchedlogv = changedlogv --more
	touchedlogvmine = changedlogvmine --more
	touchedlg = changedlg --more
	touchedlgmine = changedlgmine --more
	touchedlc = changedlc --more
	touchedlcmine = changedlcmine --more
	touchedlcl = changedlcl --more
	touchedlclmine = changedlclmine --more
	touchedfiles = changedfiles --more
	touchedversions = changedversions --more
	touchedtags = changedtags --more

	wherelastlogg = logg --max-count 1	; Log only the last commit where the log message matches the specified text or /regexp/.
	wherelastloggmine = mine-command -2 wherelastlogg ME
	wherelastloggv = loggv --max-count 1
	wherelastloggvmine = mine-command -2 wherelastloggv ME
	wherelastshowg = logg --max-count 1 --patch
	wherelastshowgmine = mine-command -2 wherelastshowg ME
	wherelastloggfiles = loggfiles --max-count 1
	wherelastloggversion = findcontained wherelastlogg annotated
	wherelastloggtag = findcontained wherelastlogg describe

	wherelastchangedlog = changed-command 'log --max-count 1'	; wherelastchangedlog is like changedlog but only shows a single log (i.e. the last such commit).
	wherelastchangedlogmine = mine-command -2 wherelastchangedlog ME
	wherelastchangedlogv = changed-command 'logv --max-count 1'
	wherelastchangedlogvmine = mine-command -2 wherelastlogv ME
	wherelastchangedshow = changed-command 'log --max-count 1 --patch'
	wherelastchangedshowmine = mine-command -2 wherelastshow ME
	wherelastchangedfiles = changed-command 'logfilenames --max-count 1'
	wherelastchangedversion = findcontained wherelastchangedlog annotated
	wherelastchangedtag = findcontained wherelastchangedlog describe

	wherelasttouchedlog = changed-command 'log --max-count 1' --more	; wherelasttouchedlog is like touchedlog but only shows a single log (i.e. the last such commit).
	wherelasttouchedlogmine = mine-command -3 wherelasttouchedlog --more ME
	wherelasttouchedlogv = changed-command 'logv --max-count 1' --more
	wherelasttouchedlogvmine = mine-command -3 wherelastlogv --more ME
	wherelasttouchedshow = changed-command 'log --max-count 1 --patch' --more
	wherelasttouchedshowmine = mine-command -3 wherelastshow --more ME
	wherelasttouchedfiles = changed-command 'logfilenames --max-count 1' --more
	wherelasttouchedversion = findcontained wherelasttouchedlog annotated --more
	wherelasttouchedtag = findcontained wherelasttouchedlog describe --more

	whereintroducedlogg = rnlogg --max-count 1	; Log only the first ever commit where the log message matches the specified text or /regexp/.
	whereintroducedloggmine = mine-command -2 whereintroducedlogg ME
	whereintroducedloggv = rnloggv --max-count 1
	whereintroducedloggvmine = mine-command -2 whereintroducedloggv ME
	whereintroducedshowg = rnlogg --max-count 1 --patch
	whereintroducedshowgmine = mine-command -2 wherelastshowg ME
	whereintroducedloggfiles = rnloggfilenames --max-count 1
	whereintroducedloggversion = findcontained wherelastlogg annotated
	whereintroducedloggtag = findcontained wherelastlogg describe

	whereintroducedchangelog = changed-command 'rnlog --reverse --max-count 1'	; whereintroducedchangelog is like changedlog but only shows a single log (i.e. the first commit that introduced the change).
	whereintroducedchangelogmine = mine-command -2 whereintroducedchangelog ME
	whereintroducedchangelogv = changed-command 'rnlogv --reverse --max-count 1'
	whereintroducedchangelogvmine = mine-command -2 whereintroducedlogv ME
	whereintroducedchangeshow = changed-command 'rnshow --reverse --max-count 1'
	whereintroducedchangeshowmine = mine-command -2 whereintroducedshow ME
	whereintroducedchangefiles = changed-command 'rnlogfilenames --reverse --max-count 1'
	whereintroducedchangeversion = findcontained whereintroducedchangelog annotated
	whereintroducedchangetag = findcontained whereintroducedchangelog describe

	whereintroducedtouchedlog = whereintroducedchangelog --more	; whereintroducedtouchedlog is like touchedlog but only shows a single log (i.e. the first commit that introduced the change).
	whereintroducedtouchedlogmine = whereintroducedchangelogmine --more
	whereintroducedtouchedlogv = whereintroducedchangelogv --more
	whereintroducedtouchedlogvmine = whereintroducedchangelogvmine --more
	whereintroducedtouchedshow = whereintroducedchangeshow --more
	whereintroducedtouchedshowmine = whereintroducedchangeshowmine --more
	whereintroducedtouchedfiles = whereintroducedchangefiles --more
	whereintroducedtouchedversion = whereintroducedchangeversion --more
	whereintroducedtouchedtag = whereintroducedchangetag --more


	lc = whatchanged -m -p ; Logs together with diff between commits.
	lc1 = lc --max-count 1
	lc2 = lc --max-count 2
	lc3 = lc --max-count 3
	lc4 = lc --max-count 4
	lc5 = lc --max-count 5
	lc6 = lc --max-count 6
	lc7 = lc --max-count 7
	lcx = lc --max-count 12
	lcq = lc --max-count 25
	lcz = lc --max-count 100
	lcf = whatchanged -m --first-parent -p ; Logs together with diff between commits, but only from the "main branch" perspective, skipping commits that come from merged branches. This makes sense only when following a strict policy of merging all topic branches when staying on a single integration branch.
	;; lcl: Show separate log and diff of each commit (instead of concatenating all commits), paged through with less. Limit the number of commits via -<number> / -n <number> / --max-count <number> if the default of 20 isn't right.
	lcl1 = lcl --max-count 1
	lcl2 = lcl --max-count 2
	lcl3 = lcl --max-count 3
	lcl4 = lcl --max-count 4
	lcl5 = lcl --max-count 5
	lcl6 = lcl --max-count 6
	lcl7 = lcl --max-count 7
	lclx = lcl --max-count 12
	lclq = lcl --max-count 25
	lclz = lcl --max-count 100
	;; lclg: Show separate log and diff of commits where log messages match the specified text or /regexp/ (instead of concatenating all commits), paged through with less. Limit the number of commits via -<number> / -n <number> / --max-count <number> if the default of 20 isn't right.
	lclf = lcl --first-parent
	lcmine = mine-command -2 lc ME ; Logs together with diff between commits, filtered for my personal commits.
	lcteam = team-command -2 lc TEAM ; Logs together with diff between commits, filtered for my team's commits.
	lclastby = dashdash-default-command : others-command -3 byuserrange-command lc AUTHORS : ; Logs together with diff between commits of the last consecutive commit(s) done by the passed or queried user(s)
	lcby = dashdash-default-command : others-command -2 lc AUTHORS : ; Logs together with diff between commits, filtered for that user(s)' commits.
	lcfmine = lcmine --first-parent
	lcfteam = lcteam --first-parent
	lcfby = dashdash-default-command : others-command -3 lc --first-parent AUTHORS :
	lcr = lc --submodule=diff	; Recursive diff also showing all changes in submodules.
	lcr1 = lcr --max-count 1
	lcr2 = lcr --max-count 2
	lcr3 = lcr --max-count 3
	lcr4 = lcr --max-count 4
	lcr5 = lcr --max-count 5
	lcr6 = lcr --max-count 6
	lcr7 = lcr --max-count 7
	lcrx = lcr --max-count 12
	lcrq = lcr --max-count 25
	lcrz = lcr --max-count 100
	lcrl = lcl --submodule=diff ; Logs together with diff between commits including submodules, each commit (as a set of main repo and submodules; use lcl + sublcl for finer granularity) separately, paged through with less.
	;; show: Log together with diff for a particular commit; like lc but will not log further commits.
	;; showl: Show separate log together with diff of each passed revision (instead of concatenating all commits), paged through with less.
	showw = -c color.diff.old=red\\ reverse -c color.diff.new=green\\ reverse wrapper show --color-words	; use word-based (not line-based) diff granularity
	showc = -c color.diff.old=red\\ reverse -c color.diff.new=green\\ reverse wrapper show --color-words --word-diff-regex=.	; use character-based diff granularity
	showa = -c color.diff.old=red\\ reverse -c color.diff.new=green\\ reverse wrapper show --color-words --word-diff-regex=(^|[^[:alnum:]_])[[:alnum:]_]+([^[:alnum:]_]|$)|[^[:space:]]	; use alnum-based diff granularity

	;; showselected: Log together with diff for the commit interactively chosen from the commits in the passed <range>.
	ss = showselected
	ss1 = showselected --max-count 1
	ss2 = showselected --max-count 2
	ss3 = showselected --max-count 3
	ss4 = showselected --max-count 4
	ss5 = showselected --max-count 5
	ss6 = showselected --max-count 6
	ss7 = showselected --max-count 7
	ssx = showselected --max-count 12
	ssq = showselected --max-count 25
	ssz = showselected --max-count 100
	ssg = selectedgcommit-command --single-only -2 show COMMITS	; Log together with diff for the commit interactively chosen from the commits where log messages match the specified text or /regexp/.
	ssst = stfiles-command showselected
	sslastst = stfiles-command selectedcommit-command --accept-single --single-only -2 show COMMITS --max-count 1
	ssst1 = ssst --max-count 1
	ssst2 = ssst --max-count 2
	ssst3 = ssst --max-count 3
	ssst4 = ssst --max-count 4
	ssst5 = ssst --max-count 5
	ssst6 = ssst --max-count 6
	ssst7 = ssst --max-count 7
	ssstx = ssst --max-count 12
	ssstq = ssst --max-count 25
	ssstz = ssst --max-count 100
	ssi = stifiles-command showselected
	sslasti = stifiles-command selectedcommit-command --accept-single --single-only -2 show COMMITS --max-count 1
	ssi1 = ssi --max-count 1
	ssi2 = ssi --max-count 2
	ssi3 = ssi --max-count 3
	ssi4 = ssi --max-count 4
	ssi5 = ssi --max-count 5
	ssi6 = ssi --max-count 6
	ssi7 = ssi --max-count 7
	ssix = ssi --max-count 12
	ssiq = ssi --max-count 25
	ssiz = ssi --max-count 100
	ss-i = stIfiles-command showselected
	sslast-i = stIfiles-command selectedcommit-command --accept-single --single-only -2 show COMMITS --max-count 1
	ss-i1 = ss-i --max-count 1
	ss-i2 = ss-i --max-count 2
	ss-i3 = ss-i --max-count 3
	ss-i4 = ss-i --max-count 4
	ss-i5 = ss-i --max-count 5
	ss-i6 = ss-i --max-count 6
	ss-i7 = ss-i --max-count 7
	ss-ix = ss-i --max-count 12
	ss-iq = ss-i --max-count 25
	ss-iz = ss-i --max-count 100

	ssw = showwselected
