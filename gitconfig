	b2lcorrecti = branch-command --keep-position rev-range --revision @{-2} --end-revision BRANCH -2 correcti RANGE
	b2lcorrect-i = branch-command --keep-position rev-range --revision @{-2} --end-revision BRANCH -2 correct-i RANGE
	b2lfixup = rev-range --revision @{-2} --end-revision HEAD -2 fixupselected RANGE
	b2lfixupg = rev-range --revision @{-2} --end-revision HEAD -2 fixupselectedg RANGE
