	b3rcommitlikei = rev-range-this-branch --branch-number 3 -2 commitlikei RANGE
	b3rcommitlike-i = rev-range-this-branch --branch-number 3 -2 commitlike-i RANGE
	b3rcommitrelate = rev-range-this-branch --branch-number 3 -2 commitrelateselected RANGE
	b3rcommitrelateg = rev-range-this-branch --branch-number 3 -2 commitrelateselectedg RANGE
	b3rcommitrelatest = rev-range-this-branch --branch-number 3 -2 commitrelatest RANGE
	b3rcommitrelatei = rev-range-this-branch --branch-number 3 -2 commitrelatei RANGE
	b3rcommitrelate-i = rev-range-this-branch --branch-number 3 -2 commitrelate-i RANGE
	;b3rrb is a no-op, because it iterates over the current range without touching fixups.
	b3rrbi = rev-range-this-branch --branch-number 3 --no-range -3 rebase -i RANGE	; Rebase on top of the current branch and two preceding branches.
	b3rrbni = rev-range-this-branch --branch-number 3 --no-range -2 rbni RANGE
	b3rmove-to-branch = rev-range-this-branch --branch-number 3 --no-range +1 RANGE uncommit-to-branch --exclude-commit
	b3runcommit-to-branch = rev-range-this-branch --branch-number 3 -- selectedcommit-command --single-only --range-is-last -3 uncommit-to-branch --from COMMITS RANGE
	b3rcreatebranch = rev-range-this-branch --branch-number 3 -2 createbranchselected RANGE
	b3rdetach = rev-range-this-branch --branch-number 3 -2 detachselected RANGE
	b3rdetachg = rev-range-this-branch --branch-number 3 -2 detachselectedg RANGE
	b3rwipe = rev-range-this-branch --branch-number 3 -2 wipetoselected RANGE
	b3rwipeg = rev-range-this-branch --branch-number 3 -2 wipetoselectedg RANGE
	b3rbase = rev-range-this-branch --branch-number 3 --no-range -3 name-rev --name-only RANGE
	b3rbaselg = rev-range-this-branch --branch-number 3 --no-range -2 lg1 RANGE
	b3rpred = rev-range-this-branch --branch-number 3 --no-range --one-more -3 name-rev --name-only RANGE
	b3rpredlg = rev-range-this-branch --branch-number 3 --no-range --one-more -2 lg1 RANGE
	b3rcat = rev-range-this-branch --branch-number 3 --no-range -2 cat RANGE
	b3rwhocreatedthosechangedfiles = files-command --source-command b3rfiles whocreated
	b3rwhocreatedhere = rev-range-this-branch --branch-number 3 -2 whocreated RANGE
	b3rwhodidthosechangedfiles = files-command --source-command b3rfiles whodid
	b3rwhodidhere = rev-range-this-branch --branch-number 3 -2 whodid RANGE
	b3rwhoownsthosechangedfiles = files-command --source-command b3rfiles whoowns
	; b3rwhoownsthechanges even with the changed files would not show useful data
	b3rwhocontributedthosechangedfiles = files-command --source-command b3rfiles whocontributed
	b3rwhocontributedhere = rev-range-this-branch --branch-number 3 -2 whocontributed RANGE
	b3rwhowhen = rev-range-this-branch --branch-number 3 -2 whowhen RANGE
	b3rwhofirst = rev-range-this-branch --branch-number 3 -2 whofirst RANGE
	b3rwholast = rev-range-this-branch --branch-number 3 -2 wholast RANGE
