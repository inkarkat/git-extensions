	b2lcorrect = branch-command --keep-position rev-range --revision @{-2} --end-revision BRANCH -2 correctselected RANGE
	b2lcorrectg = branch-command --keep-position rev-range --revision @{-2} --end-revision BRANCH -2 correctselectedg RANGE
