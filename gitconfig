	b2lamendrelate-i = branch-command --keep-position rev-range --revision @{-2} --end-revision BRANCH -2 amendrelate-i RANGE
	b2lrb = rev-range --revision @{-2} --no-range -2 rebase RANGE	; Rebase on top of the second-last checked out branch.
	b2lrbi = rev-range --revision @{-2} --no-range -3 rebase -i RANGE
	b2lrbni = rev-range --revision @{-2} --no-range -2 rbni RANGE
	b2lmove-to-branch = rev-range --revision @{-2} --no-range +1 RANGE uncommit-to-branch --exclude-commit
	b2luncommit-to-branch = branch-command --keep-position rev-range --revision @{-2} --keep-position selectedcommit-command --single-only --range-is-last -4 uncommit-to-branch --from COMMITS RANGE
	b2lcreatebranch = branch-command --keep-position rev-range --revision @{-2} -2 createbranchselected RANGE
	b2ldetach = branch-command --keep-position rev-range --revision @{-2} -2 detachselected RANGE
	b2ldetachg = branch-command --keep-position rev-range --revision @{-2} -2 detachselectedg RANGE
	b2lwipe = branch-command --keep-position rev-range --revision @{-2} -2 wipetoselected RANGE
	b2lwipeg = branch-command --keep-position rev-range --revision @{-2} -2 wipetoselectedg RANGE
	b2lbase = rev-range --revision @{-2} --no-range -3 name-rev --name-only RANGE
	b2lbaselg = rev-range --revision @{-2} --no-range -2 lg1 RANGE
	b2lpred = rev-range --revision @{-2} --no-range --one-more -3 name-rev --name-only RANGE
	b2lpredlg = rev-range --revision @{-2} --no-range --one-more -2 lg1 RANGE
