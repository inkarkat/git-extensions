	;; beforeby...: Log variants that cover the very first commit from the passed author / committer(s) (or the author (+ committer if different) from the start revision) to the current / passed -r|--revision REVISION.
	beforebyd = beforeby-command --with-log-args --with-range ... -2 diff USERRANGE
	beforebydsta = beforeby-command --with-log-args --with-range ... -2 diff --shortstat USERRANGE
	beforebydstat = beforeby-command --with-log-args --with-range ... -3 diff --stat --compact-summary USERRANGE
	beforebydstatbyeach = beforeby-command --with-log-args -2 logstatbyeach USERRANGE
	beforebydstabyeach = beforeby-command --with-log-args -2 logstabyeach USERRANGE
	beforebydl = beforeby-command --with-log-args --with-range ... -2 dl USERRANGE
	beforebydt = beforeby-command --with-log-args --with-range ... -2 difftool USERRANGE
	beforebydy = beforeby-command --with-log-args --with-range ... -2 diffy USERRANGE
	beforebyd-y = beforeby-command --with-log-args --with-range ... -2 diffY USERRANGE
	beforebydr = beforeby-command --with-log-args --with-range ... -2 dr USERRANGE
	beforebydrl = beforeby-command --with-log-args --with-range ... -2 drl USERRANGE
	beforebyma = beforeby-command -2 format-patch USERRANGE
	beforebylogg = beforeby-command --with-log-args-except-last -2 logg USERRANGE
	beforebyloggmine = beforeby-command --with-log-args-except-last -2 loggmine USERRANGE
	beforebyloggv = beforeby-command --with-log-args-except-last -2 loggv USERRANGE
	beforebyloggvmine = beforeby-command --with-log-args-except-last -2 loggvmine USERRANGE
	beforebylgg = beforeby-command --with-log-args-except-last -2 lgg USERRANGE
	beforebylggmine = beforeby-command --with-log-args-except-last -2 lggmine USERRANGE
