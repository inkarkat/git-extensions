	bpwhodidhere = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 whodid RANGE
	bpwhodidfhere = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 whodidf RANGE
	bpwhoownsthosechangedfiles = branch-command --keep-position files-command --source-command 'bpfiles --branch BRANCH' whoowns
	; bpwhoownsthechanges even with the changed files would not show useful data
	bpwhocontributedthosechangedfiles = branch-command --keep-position files-command --source-command 'bpfiles --branch BRANCH' whocontributed
