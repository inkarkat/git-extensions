[alias]
	stablerb = rebase stable    ; Rebase my commits on top of the stable branch.
	blrb = branch-command --keep-position rev-range --revision @{-1} --end-revision BRANCH -2 rebase RANGE    ; Rebase my commits on top of the last checked-out branch; same as "rebase @{-1}".
	b2lrb = branch-command --keep-position rev-range --revision @{-2} --end-revision BRANCH -2 rebase RANGE    ; Rebase my commits on top of the second-last checked-out branch; same as "rebase @{-2}".
	bprb = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 rebase RANGE    ; Rebase my commits on top of where the tip of the branch was previously; same as "rebase @{1}".
	b2prb = branch-command --keep-position rev-range --revision @{2} --end-revision BRANCH -2 rebase RANGE    ; Rebase my commits on top of where the tip of the branch was second-last; same as "rebase @{2}".
	tobrb = branch-command --mandatory-branch -2 rebase BRANCH    ; Rebase my commits on top of the passed branch; same as "rebase BRANCH".
	psrb = rev-range-to-previous-stack --no-range -2 rebase RANGE ; Rebase my commits on top of the previous stacked branch.

	;; rev-range: Run a Git command with a revision range from the passed revision up to HEAD.
	;; rev-range-this-branch: Run a Git command with a revision range that covers the current / passed via -b|--branch branch (up to another reference).
	;; rev-range-incoming: Run a Git command with the revision range of the commits that already exist in remote on the current branch, but not yet in the local branch.
	;; rev-range-outgoing: Run a Git command with the revision range of the commits that already exist on the current branch, but not yet in the corresponding remote branch.

	;; abr...: Log variants that only cover the current / passed via -b|--branch branch (as tagged or referenced anywhere in the history (also by remotes))
	abrd = rev-range-this-branch --any --with-range ... -2 diff RANGE
	abrds = branch-command --keep-position files-command --source-command 'abrfiles --branch BRANCH' rev-range-this-branch --any --branch BRANCH --with-range ... -2 diffselected RANGE
	abrdss = rev-range-this-branch --any --keep-position selectedcommit-command --single-only --with-range-from-end ^... --range-is-last -3 diff COMMITS RANGE
	abrdsta = rev-range-this-branch --any --with-range ... -2 dsta RANGE
	abrdstat = rev-range-this-branch --any --with-range ... -2 dstat RANGE
	abrdstatbyeach = rev-range-this-branch --any -2 logstatbyeach RANGE
	abrdstabyeach = rev-range-this-branch --any -2 logstabyeach RANGE
	abrdl = rev-range-this-branch --any --with-range ... -2 dl RANGE
	abrdt = rev-range-this-branch --any --with-range ... -2 difftool RANGE
	abrdb = rev-range-this-branch --any --with-range ... -2 db RANGE
	abrdw = rev-range-this-branch --any --with-range ... -2 dw RANGE
	abrdc = rev-range-this-branch --any --with-range ... -2 dc RANGE
	abrda = rev-range-this-branch --any --with-range ... -2 da RANGE
	abrdy = rev-range-this-branch --any --with-range ... -2 diffy RANGE
	abrd-y = rev-range-this-branch --any --with-range ... -2 diffY RANGE
	abrad = rev-range-this-branch --any --with-range ... -2 annotatediff RANGE
	abradp = rev-range-this-branch --any --keep-position selectedcommit-command --single-only --range-is-last -3 adp COMMITS RANGE
	abrdr = rev-range-this-branch --any --with-range ... -2 dr RANGE
	abrdrl = rev-range-this-branch --any --with-range ... -2 drl RANGE
	abrma = rev-range-this-branch --any -2 format-patch RANGE
	abrlogg = rev-range-this-branch --any -2 logg RANGE
	abrloggmine = rev-range-this-branch --any -2 loggmine RANGE
	abrlogvg = rev-range-this-branch --any -2 logvg RANGE
	abrlogvgmine = rev-range-this-branch --any -2 logvgmine RANGE
	abrlgg = rev-range-this-branch --any -2 lgg RANGE
	abrlggmine = rev-range-this-branch --any -2 lggmine RANGE
	abrlcg = rev-range-this-branch --any -2 lcg RANGE
	abrlcgmine = rev-range-this-branch --any -2 lcgmine RANGE
	abrlclg = rev-range-this-branch --any -2 lclg RANGE
	abrlclgmine = rev-range-this-branch --any -2 lclgmine RANGE
