	trbamendrelatei = rev-range-to-pre-rebase -2 amendrelatei RANGE
	trbamendrelate-i = rev-range-to-pre-rebase -2 amendrelate-i RANGE
	;trbrb is a no-op, because it iterates over the current range without touching fixups.
	trbrbi = rev-range-to-pre-rebase --keep-position selectedcommit-command --single-only --range-is-last -5 previouscommit-command --commit COMMITS rbi RANGE
	trbrbni = rev-range-to-pre-rebase --keep-position selectedcommit-command --single-only --range-is-last -5 previouscommit-command --commit COMMITS rbni RANGE
	trbsegregatecommits = rev-range-to-pre-rebase --keep-position selectedcommit-command --single-only --range-is-last --explicit-file-args -5 previouscommit-command --commit COMMITS segregatecommits RANGE
	trbsegregateandbifurcate = rev-range-to-pre-rebase --keep-position selectedcommit-command --single-only --range-is-last --explicit-file-args -5 previouscommit-command --commit COMMITS segregateandbifurcate RANGE
	trbbifurcate = rev-range-to-pre-rebase --keep-position selectedcommit-command --single-only --range-is-last -5 previouscommit-command --commit COMMITS bifurcate RANGE
	trbrblastfixup = rev-range-to-pre-rebase --one-more -2 rblastfixup RANGE
	trbmove-to-branch = rev-range-to-pre-rebase --no-range +1 RANGE uncommit-to-branch --exclude-commit
	trbuncommit-to-stash = "rev-range-to-pre-rebase --keep-position selectedcommit-command --pass-file-args --range-is-last -5 uncommit-to-branch --commits COMMITS ; RANGE"
	trbuncommit-to-branch = rev-range-to-pre-rebase --keep-position selectedcommit-command --single-only --range-is-last -4 uncommit-to-branch --from COMMITS RANGE
	trbcreatebr = rev-range-to-pre-rebase -2 createbrselected RANGE
	trbstackbrfrom = rev-range-to-pre-rebase -2 stackbrfromselected RANGE
	trbdetach = rev-range-to-pre-rebase --one-more -2 detachselected RANGE
	trbdetachg = rev-range-to-pre-rebase -2 detachg RANGE
	trbdetachchanged = rev-range-to-pre-rebase -2 detachchanged RANGE
	trbdetachtouched = rev-range-to-pre-rebase -2 detachtouched RANGE
