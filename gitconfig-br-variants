	oinuncommit-to-branch = rev-range-incoming origin --keep-position selectedcommit-command --single-only --range-is-last -4 uncommit-to-branch --from COMMITS RANGE
	oincreatebr = rev-range-incoming origin --one-more-output -2 createbrselected RANGE
	oindetach = rev-range-incoming origin --one-more-output -2 detachselected RANGE
	oindetachg = rev-range-incoming origin --one-more-output -2 detachselectedg RANGE
	oinwipe = rev-range-incoming origin --one-more-output -2 wipetoselected RANGE
	oinwipeg = rev-range-incoming origin --one-more-output -2 wipetoselectedg RANGE
	; oinbase would always print "origin/<branch>".
	oinbaselg = rev-range-incoming origin --no-range -2 lg1 RANGE
	oinbases = rev-range-incoming origin --no-range -2 show RANGE
	oinpred = rev-range-incoming origin --no-range --one-more -3 name-rev --name-only RANGE
	oinpredlg = rev-range-incoming origin --no-range --one-more -2 lg1 RANGE
	oinpreds = rev-range-incoming origin --no-range --one-more -2 show RANGE
	oinwhocreatedthosechangedfiles = branch-command --keep-position files-command --source-command 'oinfiles --branch BRANCH' whocreated
	oinwhocreatedhere = rev-range-incoming origin -2 whocreated RANGE
	oinwholasttouchedthosechangedfiles = branch-command --keep-position files-command --source-command 'oinfiles --branch BRANCH' wholasttouched
	oinwholasttouchedhere = rev-range-incoming origin -2 wholasttouched RANGE
