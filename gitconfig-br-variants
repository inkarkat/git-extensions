	bpcommitrelateg = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 commitrelateg RANGE
	bpcommitrelatechanged = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 commitrelatechanged RANGE
	bpcommitrelatetouched = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 commitrelatetouched RANGE
	bpcommitrelatest = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 commitrelatest RANGE
	bpcommitrelatei = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 commitrelatei RANGE
	bpcommitrelate-i = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 commitrelate-i RANGE
	bpamendrelate = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 amendrelateselected RANGE
	bpamendrelateg = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 amendrelateg RANGE
	bpamendrelatechanged = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 amendrelatechanged RANGE
	bpamendrelatetouched = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 amendrelatetouched RANGE
	bpamendrelatest = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 amendrelatest RANGE
	bpamendrelatei = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 amendrelatei RANGE
	bpamendrelate-i = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 amendrelate-i RANGE
	bprb = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 rebase RANGE    ; Rebase my commits on top of where the tip of the branch was previously; same as "rebase @{1}".
	bprbi = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH --keep-position selectedcommit-command --single-only --range-is-last -5 previouscommit-command --commit COMMITS rbi RANGE
	bprbni = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH --keep-position selectedcommit-command --single-only --range-is-last -5 previouscommit-command --commit COMMITS rbni RANGE
	bpsegregatecommits = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH --keep-position selectedcommit-command --single-only --range-is-last --explicit-file-args -5 previouscommit-command --commit COMMITS segregatecommits RANGE
	bpsegregateandbifurcate = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH --keep-position selectedcommit-command --single-only --range-is-last --explicit-file-args -5 previouscommit-command --commit COMMITS segregateandbifurcate RANGE
	bpbifurcate = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH --keep-position selectedcommit-command --single-only --range-is-last -5 previouscommit-command --commit COMMITS bifurcate RANGE
	bprblastfixup = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH --one-more -2 rblastfixup RANGE
	bpmove-to-branch = uncommit-to-branch --exclude-commit @{1}
	bpuncommit-to-stash = "branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH --keep-position selectedcommit-command --pass-file-args --range-is-last -5 uncommit-to-branch --commits COMMITS ; RANGE"
	bpuncommit-to-branch = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH --keep-position selectedcommit-command --single-only --range-is-last -4 uncommit-to-branch --from COMMITS RANGE
	bpcreatebr = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 createbrselected RANGE
	bpstackbrfrom = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 stackbrfromselected RANGE
	bpdetach = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH --one-more -2 detachselected RANGE
	bpdetachg = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 detachg RANGE
	bpdetachchanged = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 detachchanged RANGE
	bpdetachtouched = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 detachtouched RANGE
	bpwipe = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH --one-more -2 wipetoselected RANGE
	bpwipeg = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 wipetog RANGE
	bpwipechanged = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 wipetochanged RANGE
	bpwipetouched = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 wipetotouched RANGE
	bpbase = name-rev --name-only @{1}
	bpbaselg = lg1 @{1}
	bpbases = show @{1}
	bppred = rev-range --revision @{1} --no-range --one-more -3 name-rev --name-only RANGE
	bppredlg = rev-range --revision @{1} --no-range --one-more -2 lg1 RANGE
	bppreds = rev-range --revision @{1} --no-range --one-more -2 show RANGE
	bpcat = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 catselectedonemore RANGE
	bpcp = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 cpselectedonemore RANGE
	bpwhocreatedthosechangedfiles = branch-command --keep-position files-command --source-command 'bpfiles --branch BRANCH' whocreated
	bpwhocreatedhere = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 whocreated RANGE
	bpwholasttouchedthosechangedfiles = branch-command --keep-position files-command --source-command 'bpfiles --branch BRANCH' wholasttouched
	bpwholasttouchedhere = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 wholasttouched RANGE
