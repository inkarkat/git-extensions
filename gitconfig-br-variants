	; dtrb is a no-op, because it iterates over the current range without touching fixups.
	dtrbi = tagdelta-command --keep-position rev-range --revision TAG2 --keep-position selectedcommit-command --single-only --range-is-last -5 previouscommit-command --commit COMMITS rbi RANGE
	dtrbni = tagdelta-command --keep-position rev-range --revision TAG2 --keep-position selectedcommit-command --single-only --range-is-last -5 previouscommit-command --commit COMMITS rbni RANGE
	dtsegregatecommits = tagdelta-command --keep-position rev-range --revision TAG2 --keep-position selectedcommit-command --single-only --range-is-last --explicit-file-args -5 previouscommit-command --commit COMMITS segregatecommits RANGE
	dtsegregateandbifurcate = tagdelta-command --keep-position rev-range --revision TAG2 --keep-position selectedcommit-command --single-only --range-is-last --explicit-file-args -5 previouscommit-command --commit COMMITS segregateandbifurcate RANGE
	dtbifurcate = tagdelta-command --keep-position rev-range --revision TAG2 --keep-position selectedcommit-command --single-only --range-is-last -5 previouscommit-command --commit COMMITS bifurcate RANGE
	dtrblastfixup = tagdelta-command --keep-position rev-range --revision TAG2 --one-more -2 rblastfixup RANGE
	dtmove-to-branch = tagdelta-command +1 TAG2 uncommit-to-branch --exclude-commit
	dtuncommit-to-stash = "tagdelta-command --keep-position rev-range --revision TAG2 --keep-position selectedcommit-command --pass-file-args --range-is-last -5 uncommit-to-branch --commits COMMITS ; RANGE"
	dtuncommit-to-branch = tagdelta-command --keep-position rev-range --revision TAG2 --keep-position selectedcommit-command --single-only --range-is-last -4 uncommit-to-branch --from COMMITS RANGE
	dtcreatebr = tagdelta-command --keep-position rev-range --revision TAG2 -2 createbrselected RANGE
	dtstackbrfrom = tagdelta-command --keep-position rev-range --revision TAG2 -2 stackbrfromselected RANGE
	dtdetach = tagdelta-command --keep-position rev-range --revision TAG2 --one-more -2 detachselected RANGE
	dtdetachg = tagdelta-command --keep-position rev-range --revision TAG2 -2 detachg RANGE
	dtdetachchanged = tagdelta-command --keep-position rev-range --revision TAG2 -2 detachchanged RANGE
	dtdetachtouched = tagdelta-command --keep-position rev-range --revision TAG2 -2 detachtouched RANGE
	dtwipe = tagdelta-command --keep-position rev-range --revision TAG2 --one-more -2 wipetoselected RANGE
	dtwipeg = tagdelta-command --keep-position rev-range --revision TAG2 -2 wipetog RANGE
	dtwipechanged = tagdelta-command --keep-position rev-range --revision TAG2 -2 wipetochanged RANGE
	dtwipetouched = tagdelta-command --keep-position rev-range --revision TAG2 -2 wipetotouched RANGE
	dtbase = tagdelta-command -3 name-rev --name-only TAG2
	dtbaselg = tagdelta-command -2 lg1 TAG2
	dtbases = tagdelta-command -2 show TAG2
	dtpred = tagdelta-command --keep-position rev-range --revision TAG2 --no-range --one-more -3 name-rev --name-only RANGE
	dtpredlg = tagdelta-command --keep-position rev-range --revision TAG2 --no-range --one-more -2 lg1 RANGE
	dtpreds = tagdelta-command --keep-position rev-range --revision TAG2 --no-range --one-more -2 show RANGE
	dtcat = tagdelta-command --keep-position rev-range --revision TAG2 --end-revision TAG1 -2 catselectedonemore RANGE
	dtcp = tagdelta-command --keep-position rev-range --revision TAG2 --end-revision TAG1 -2 cpselectedonemore RANGE
	dtwhocreatedthosechangedfiles = argsyphon-command '' 'reverse|version-tags-only|no-version-tags|sort-by-age' 'rodie' 'revision|tag-offset|tag-delta|include|exclude' --keep-position files-command --source-command 'dtfiles 'QARGS whocreated
	dtwhocreatedhere = tagdelta-command --keep-position rev-range --revision TAG2 --end-revision TAG1 -2 whocreated RANGE
	dtwholasttouchedthosechangedfiles = argsyphon-command '' 'reverse|version-tags-only|no-version-tags|sort-by-age' 'rodie' 'revision|tag-offset|tag-delta|include|exclude' --keep-position files-command --source-command 'dtfiles 'QARGS wholasttouched
	dtwholasttouchedhere = tagdelta-command --keep-position rev-range --revision TAG2 --end-revision TAG1 -2 wholasttouched RANGE
	dtwhodidthosechangedfiles = argsyphon-command '' 'reverse|version-tags-only|no-version-tags|sort-by-age' 'rodie' 'revision|tag-offset|tag-delta|include|exclude' --keep-position files-command --source-command 'dtfiles 'QARGS whodid
	dtwhodidhere = tagdelta-command --keep-position rev-range --revision TAG2 --end-revision TAG1 -2 whodid RANGE
	dtwhodidfhere = tagdelta-command --keep-position rev-range --revision TAG2 --end-revision TAG1 -2 whodidf RANGE
	dtwhoownsthosechangedfiles = argsyphon-command '' 'reverse|version-tags-only|no-version-tags|sort-by-age' 'rodie' 'revision|tag-offset|tag-delta|include|exclude' --keep-position files-command --source-command 'dtfiles 'QARGS whoowns
	; dtwhoownsthechanges even with the changed files would not show useful data
	dtwhocontributedthosechangedfiles = argsyphon-command '' 'reverse|version-tags-only|no-version-tags|sort-by-age' 'rodie' 'revision|tag-offset|tag-delta|include|exclude' --keep-position files-command --source-command 'dtfiles 'QARGS whocontributed
	dtwhocontributedhere = tagdelta-command --keep-position rev-range --revision TAG2 --end-revision TAG1 -2 whocontributed RANGE
