	abramendrelate-i = rev-range-this-branch --any -2 amendrelate-i RANGE
	;abrrb is a no-op, because it iterates over the current range without touching fixups.
	abrrbi = rev-range-this-branch --any --keep-position selectedcommit-command --single-only --range-is-last -5 previouscommit-command --commit COMMITS rbi RANGE
	abrrbni = rev-range-this-branch --any --keep-position selectedcommit-command --single-only --range-is-last -5 previouscommit-command --commit COMMITS rbni RANGE
	abrsegregatecommits = rev-range-this-branch --any --keep-position selectedcommit-command --single-only --range-is-last --explicit-file-args -5 previouscommit-command --commit COMMITS segregatecommits RANGE
