	lbmwhoownsthosechangedfiles = localbrrefdo-wrapper --no-git-color files-command --source-command 'brfiles --branch {}' whoowns
	; lbmwhoownsthechanges even with the changed files would not show useful data
	lbmwhocontributedthosechangedfiles = localbrrefdo-wrapper --no-git-color files-command --source-command 'brfiles --branch {}' whocontributed
	lbmwhocontributedhere = localbrrefdo-wrapper --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -2 whocontributed RANGE
