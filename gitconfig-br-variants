	psrb = rev-range-to-previous-stack --no-range -2 rebase RANGE ; Rebase my commits on top of the previous stacked branch.
	psrbi = rev-range-to-previous-stack --keep-position selectedcommit-command --single-only --range-is-last -5 previouscommit-command --commit COMMITS rbi RANGE
	psrbni = rev-range-to-previous-stack --keep-position selectedcommit-command --single-only --range-is-last -5 previouscommit-command --commit COMMITS rbni RANGE
