	; uinfixuprb doesn't make sense; you cannot rebase stuff in a remote branch.
	; uinrb is a no-op, because it iterates over the current range without touching fixups.
	uinrbi = rev-range-incoming upstream --keep-position selectedcommit-command --single-only --range-is-last -5 previouscommit-command --commit COMMITS rbi RANGE
	uinrbni = rev-range-incoming upstream --keep-position selectedcommit-command --single-only --range-is-last -5 previouscommit-command --commit COMMITS rbni RANGE
	uinsegregatecommits = rev-range-incoming upstream --keep-position selectedcommit-command --single-only --range-is-last --explicit-file-args -5 previouscommit-command --commit COMMITS segregatecommits RANGE
	uinsegregateandbifurcate = rev-range-incoming upstream --keep-position selectedcommit-command --single-only --range-is-last --explicit-file-args -5 previouscommit-command --commit COMMITS segregateandbifurcate RANGE
	uinbifurcate = rev-range-incoming upstream --keep-position selectedcommit-command --single-only --range-is-last -5 previouscommit-command --commit COMMITS bifurcate RANGE
	uinrblastfixup = rev-range-incoming upstream --one-more -2 rblastfixup RANGE
	uinuncommit-to-stash = "rev-range-incoming upstream --keep-position selectedcommit-command --pass-file-args --range-is-last -5 uncommit-to-branch --commits COMMITS ; RANGE"
	uinuncommit-to-branch = rev-range-incoming upstream --keep-position selectedcommit-command --single-only --range-is-last -4 uncommit-to-branch --from COMMITS RANGE
	uincreatebr = rev-range-incoming upstream --one-more -2 createbrselected RANGE
	uinstackbrfrom = rev-range-incoming upstream --one-more -2 stackbrfromselected RANGE
