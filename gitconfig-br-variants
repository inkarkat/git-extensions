	msglogdistributionmine = loggrange-command --with-log-args -2 logdistributionmine LOGRANGE
	msglogdistributionteam = loggrange-command --with-log-args -2 logdistributionteam LOGRANGE
	msgemaillog = loggrange-command -3 email-command log LOGRANGE
	msgemaillc = loggrange-command -3 email-command lc LOGRANGE

	;; rev-range: Run a Git command with a revision range from the passed revision up to HEAD.
	;; rev-range-this-branch: Run a Git command with a revision range that covers the current / passed via -b|--branch branch (up to another reference).
	;; rev-range-incoming: Run a Git command with the revision range of the commits that already exist in remote on the current branch, but not yet in the local branch.
	;; rev-range-outgoing: Run a Git command with the revision range of the commits that already exist on the current branch, but not yet in the corresponding remote branch.

	;; abr...: Log variants that only cover the current / passed via -b|--branch branch (as tagged or referenced anywhere in the history (also by remotes))
	abrd = rev-range-this-branch --any --with-range ... -2 diff RANGE
	abrds = branch-command --keep-position files-command --source-command 'abrfiles --branch BRANCH' rev-range-this-branch --any --branch BRANCH --with-range ... -2 diffselected RANGE
	abrdss = rev-range-this-branch --any --keep-position selectedcommit-command --single-only --with-range-from-end ^... --range-is-last -3 diff COMMITS RANGE
	abrdsta = rev-range-this-branch --any --with-range ... -2 dsta RANGE
	abrdstat = rev-range-this-branch --any --with-range ... -2 dstat RANGE
	abrdstatbyeach = rev-range-this-branch --any -2 logstatbyeach RANGE
	abrdstabyeach = rev-range-this-branch --any -2 logstabyeach RANGE
	abrdl = rev-range-this-branch --any --with-range ... -2 dl RANGE
	abrdt = rev-range-this-branch --any --with-range ... -2 difftool RANGE
	abrdb = rev-range-this-branch --any --with-range ... -2 db RANGE
	abrdw = rev-range-this-branch --any --with-range ... -2 dw RANGE
	abrdc = rev-range-this-branch --any --with-range ... -2 dc RANGE
	abrda = rev-range-this-branch --any --with-range ... -2 da RANGE
	abrdy = rev-range-this-branch --any --with-range ... -2 diffy RANGE
