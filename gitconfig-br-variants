	bmuncommit-to-stash = "branch-command --keep-position master-command --keep-position rev-range --revision MASTER --end-revision BRANCH --keep-position selectedcommit-command --pass-file-args --range-is-last -5 uncommit-to-branch --commits COMMITS ; RANGE"
