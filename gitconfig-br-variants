	oinlgby = dashdash-default-command --with-files : rev-range-incoming origin -7 others-command --range RANGE -2 onelinelog AUTHORS RANGE :
	oinlogmine = rev-range-incoming origin -2 logmine RANGE
	oinlogteam = rev-range-incoming origin -2 logteam RANGE
	oinlogby = dashdash-default-command --with-files : rev-range-incoming origin -7 others-command --range RANGE -2 log AUTHORS RANGE :
