	abrbifurcate = rev-range-this-branch --any --keep-position selectedcommit-command --single-only --range-is-last -5 previouscommit-command --commit COMMITS bifurcate RANGE
	abrrblastfixup = rev-range-this-branch --any --one-more -2 rblastfixup RANGE
	abrmove-to-branch = rev-range-this-branch --any --no-range +1 RANGE uncommit-to-branch --exclude-commit
	abruncommit-to-stash = "rev-range-this-branch --any --keep-position selectedcommit-command --pass-file-args --range-is-last -5 uncommit-to-branch --commits COMMITS ; RANGE"
	abruncommit-to-branch = rev-range-this-branch --any --keep-position selectedcommit-command --single-only --range-is-last -4 uncommit-to-branch --from COMMITS RANGE
	abrcreatebr = rev-range-this-branch --any -2 createbrselected RANGE
	abrstackbrfrom = rev-range-this-branch --any -2 stackbrfromselected RANGE
