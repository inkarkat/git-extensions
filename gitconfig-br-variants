	stablecp = branch-command --keep-position rev-range --revision stable --end-revision BRANCH -2 cpselectedonemore RANGE
	stablewhocreatedthosechangedfiles = branch-command --keep-position files-command --source-command 'stablefiles --branch BRANCH' whocreated
	stablewhocreatedhere = branch-command --keep-position rev-range --revision stable --end-revision BRANCH -2 whocreated RANGE
	stablewholasttouchedthosechangedfiles = branch-command --keep-position files-command --source-command 'stablefiles --branch BRANCH' wholasttouched
