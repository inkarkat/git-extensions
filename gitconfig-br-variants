[alias]
	;; td...: Log variants that only cover changes committed today of the current / passed via -r|--revision REVISION.
	tdd = revision-command --keep-position today-command --revision REVISION --no-range -2 diffuntil TIMESPAN ; Show all changes from today in a single big diff.
	tdds = "!sh -c \"GIT_SELECTED_COMMAND_DEFAULT_FILES='git tdfiles' git-selected-command tdd\""
	tddsta = revision-command --keep-position today-command --revision REVISION --no-range -2 diffuntil TIMESPAN --shortstat
	tddstat = revision-command --keep-position today-command --revision REVISION --no-range -2 diffuntil TIMESPAN --stat --compact-summary
	tddstatbyeach = revision-command --keep-position today-command --revision REVISION -2 logstatbyeach TIMESPAN
	tddstabyeach = revision-command --keep-position today-command --revision REVISION -2 logstabyeach TIMESPAN
	tddl = revision-command --keep-position today-command --revision REVISION --no-range -2 diffuntill TIMESPAN ; Show all changes from today in a big diffs, separately for each changed file.
	tddt = revision-command --keep-position today-command --revision REVISION --no-range -2 difftooluntil TIMESPAN
	tddb = revision-command --keep-position today-command --revision REVISION --no-range -2 diffuntilb TIMESPAN
	tddw = revision-command --keep-position today-command --revision REVISION --no-range -2 diffuntilw TIMESPAN
