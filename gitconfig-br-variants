	stablewhoownsthosechangedfiles = branch-command --keep-position files-command --source-command 'stablefiles --branch BRANCH' whoowns
	; stablewhoownsthechanges even with the changed files would not show useful data
	stablewhocontributedthosechangedfiles = branch-command --keep-position files-command --source-command 'stablefiles --branch BRANCH' whocontributed
