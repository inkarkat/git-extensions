	uinemaillc = rev-range-incoming upstream -3 email-command lc RANGE

	;; lbr...: Log variants that cover all local branches (like br... ending with a local reference).
	lbrd = localbrrefdo-wrapper rev-range-this-branch --branch {} --with-range ... -2 diff RANGE
	lbrds = "!sh -c \"GIT_SELECTED_COMMAND_DEFAULT_FILES='git lbrfiles --no-header | sort --unique' git-selected-command lbrd\""
	lbrdsta = localbrrefdo-wrapper rev-range-this-branch --branch {} --with-range ... -2 dsta RANGE
	lbrdstat = localbrrefdo-wrapper rev-range-this-branch --branch {} --with-range ... -2 dstat RANGE
	lbrdstatbyeach = localbrrefdo-wrapper rev-range-this-branch --branch {} -2 logstatbyeach RANGE
	lbrdstabyeach = localbrrefdo-wrapper rev-range-this-branch --branch {} -2 logstabyeach RANGE
	lbrdl = localbrrefdo-wrapper rev-range-this-branch --branch {} --with-range ... -2 dl RANGE
	lbrdt = localbrrefdo-wrapper --no-git-color rev-range-this-branch --branch {} --with-range ... -2 difftool RANGE
	lbrdb = localbrrefdo-wrapper rev-range-this-branch --branch {} --with-range ... -2 db RANGE
	lbrdw = localbrrefdo-wrapper rev-range-this-branch --branch {} --with-range ... -2 dw RANGE
	lbrdc = localbrrefdo-wrapper rev-range-this-branch --branch {} --with-range ... -2 dc RANGE
	lbrda = localbrrefdo-wrapper rev-range-this-branch --branch {} --with-range ... -2 da RANGE
	lbrdy = localbrrefdo-wrapper rev-range-this-branch --branch {} --with-range ... -2 diffy RANGE
	lbrd-y = localbrrefdo-wrapper rev-range-this-branch --branch {} --with-range ... -2 diffY RANGE
	lbrdr = localbrrefdo-wrapper rev-range-this-branch --branch {} --with-range ... -2 dr RANGE
	lbrdrl = localbrrefdo-wrapper rev-range-this-branch --branch {} --with-range ... -2 drl RANGE
	lbrma = localbrrefdo-wrapper --no-git-color rev-range-this-branch --branch {} -2 format-patch RANGE
	lbrlogg = localbrrefdo-wrapper rev-range-this-branch --branch {} -2 logg RANGE
	lbrloggmine = localbrrefdo-wrapper rev-range-this-branch --branch {} -2 loggmine RANGE
	lbrloggv = localbrrefdo-wrapper rev-range-this-branch --branch {} -2 loggv RANGE
	lbrloggvmine = localbrrefdo-wrapper rev-range-this-branch --branch {} -2 loggvmine RANGE
