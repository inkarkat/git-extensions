	lbmlgby = localbrrefdo-wrapper dashdash-default-command --with-files : master-command --keep-position rev-range --revision MASTER --end-revision {} -7 others-command --range RANGE -2 onelinelog AUTHORS RANGE :
	lbmlogmine = localbrrefdo-wrapper master-command --keep-position rev-range --revision MASTER --end-revision {} -2 logmine RANGE
