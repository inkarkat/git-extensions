	oinbifurcate = rev-range-incoming origin --keep-position selectedcommit-command --single-only --range-is-last -5 previouscommit-command --commit COMMITS bifurcate RANGE
	oinrblastfixup = rev-range-incoming origin --one-more -2 rblastfixup RANGE
	oinuncommit-to-stash = "rev-range-incoming origin --keep-position selectedcommit-command --pass-file-args --range-is-last -5 uncommit-to-branch --commits COMMITS ; RANGE"
	oinuncommit-to-branch = rev-range-incoming origin --keep-position selectedcommit-command --single-only --range-is-last -4 uncommit-to-branch --from COMMITS RANGE
