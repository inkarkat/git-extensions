	trbwipetouched = rev-range-to-pre-rebase -2 wipetotouched RANGE
	trbbase = rev-range-to-pre-rebase --no-range -2 name-rev RANGE	# No --name-only; would likely show "undefined" as not on active branch any longer.
	trbbaselg = rev-range-to-pre-rebase --no-range -2 lg1 RANGE
	trbbases = rev-range-to-pre-rebase --no-range -2 show RANGE
