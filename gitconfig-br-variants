	; trbwhoownsthechanges even with the changed files would not show useful data
	trbwhocontributedthosechangedfiles = branch-command --keep-position files-command --source-command 'trbfiles --branch BRANCH' whocontributed
	trbwhocontributedhere = rev-range-to-pre-rebase -2 whocontributed RANGE
	trbwhowhatthosechangedfiles = branch-command --keep-position files-command --source-command 'trbfiles --branch BRANCH' whowhat
	trbwhowhathere = rev-range-to-pre-rebase -2 whowhat RANGE
	trbwhowhen = rev-range-to-pre-rebase -2 whowhen RANGE
	trbwhofirst = rev-range-to-pre-rebase -2 whofirst RANGE
	trbwholast = rev-range-to-pre-rebase -2 wholast RANGE
	trbwhatdid = rev-range-to-pre-rebase -2 whatdid RANGE
	trbchurn = rev-range-to-pre-rebase -2 churn RANGE
	trbactivity = rev-range-to-pre-rebase -2 activity RANGE
	trbactivitymine = rev-range-to-pre-rebase -2 activitymine RANGE
	trbactivityteam = rev-range-to-pre-rebase -2 activityteam RANGE
	trblogdistribution = rev-range-to-pre-rebase -2 logdistribution RANGE
	trblogdistributionmine = rev-range-to-pre-rebase -2 logdistributionmine RANGE
	trblogdistributionteam = rev-range-to-pre-rebase -2 logdistributionteam RANGE
	trbemaillog = rev-range-to-pre-rebase -3 email-command log RANGE
	trbemaillc = rev-range-to-pre-rebase -3 email-command lc RANGE

	;; dt...: "delta tag"; Log variants that cover the differences of revisions of HEAD / the passed -r|--revision REVISION and the first tag or of two tags (with -o|--tag-offset N or -d|--tag-delta N in between, matching -i|--include <pattern> or not -e|--exclude <pattern>).
	dtd = tagdelta-command --keep-position rev-range --revision TAG2 --end-revision TAG1 --with-range ... -2 diff RANGE
	dtds = "tagdelta-command --keep-position rev-range --revision TAG2 --end-revision TAG1 --keep-position files-command --source-exec showfiles RANGE ; diffselected --log-range RANGE"
	dtdss = tagdelta-command --keep-position rev-range --revision TAG2 --keep-position selectedcommit-command --single-only --with-range-from-end ^... --range-is-last -3 diff COMMITS RANGE
