	uinlourl = rev-range-incoming upstream -3 lourl --reverse RANGE
	uinluurl = rev-range-incoming upstream -3 luurl --reverse RANGE
	uinlourlv = rev-range-incoming upstream -3 lourlv --reverse RANGE
	uinluurlv = rev-range-incoming upstream -3 luurlv --reverse RANGE
