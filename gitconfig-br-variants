	ttwhoownsthosechangedfiles = branch-command --keep-position files-command --source-command 'ttfiles --branch BRANCH' whoowns
	; ttwhoownsthechanges even with the changed files would not show useful data
	ttwhocontributedthosechangedfiles = branch-command --keep-position files-command --source-command 'ttfiles --branch BRANCH' whocontributed
	ttwhocontributedhere = untiltag-command --reachable-from-passed-branch --keep-position branch-command --keep-position rev-range --revision TAG --end-revision BRANCH -2 whocontributed RANGE
	ttwhowhatthosechangedfiles = branch-command --keep-position files-command --source-command 'ttfiles --branch BRANCH' whowhat
