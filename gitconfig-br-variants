	;; bp...: Log variants that cover the differences of the current / passed via -b|--branch branch to where the tip of the branch was previously. Useful to see the changes that were just pulled.
	bpd = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH --with-range ... -2 diff RANGE
	bpds = branch-command --keep-position files-command --source-command 'bpfiles --branch BRANCH' rev-range --revision @{1} --end-revision BRANCH --with-range ... -2 diffselected RANGE
	bpdss = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH --keep-position selectedcommit-command --single-only --with-range-from-end ^... --range-is-last -3 diff COMMITS RANGE
	bpdsta = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH --with-range ... -2 dsta RANGE
	bpdstat = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH --with-range ... -2 dstat RANGE
	bpdstatbyeach = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 logstatbyeach RANGE
	bpdstabyeach = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -2 logstabyeach RANGE
	bpdl = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH --with-range ... -2 dl RANGE
	bpdt = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH --with-range ... -2 difftool RANGE
	bpdb = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH --with-range ... -2 db RANGE
