	psbifurcate = rev-range-to-previous-stack --keep-position selectedcommit-command --single-only --range-is-last -5 previouscommit-command --commit COMMITS bifurcate RANGE
	psrblastfixup = rev-range-to-previous-stack --one-more -2 rblastfixup RANGE
	psmove-to-branch = rev-range-to-previous-stack --no-range +1 RANGE uncommit-to-branch --exclude-commit
	psuncommit-to-stash = "rev-range-to-previous-stack --keep-position selectedcommit-command --pass-file-args --range-is-last -5 uncommit-to-branch --commits COMMITS ; RANGE"
	psuncommit-to-branch = rev-range-to-previous-stack --keep-position selectedcommit-command --single-only --range-is-last -4 uncommit-to-branch --from COMMITS RANGE
	pscreatebr = rev-range-to-previous-stack -2 createbrselected RANGE
	psstackbrfrom = rev-range-to-previous-stack -2 stackbrfromselected RANGE
