	uincompareuurl = rev-range-incoming upstream -7 rbrurl-compare-to-base --remote upstream --base-to-rev --commit-to-rev --range RANGE
	uinlghi = rev-range-incoming upstream --one-more-command 'greyonelineloghighlight lghighlight' --one-more-only-to-terminal -2 lghi RANGE
