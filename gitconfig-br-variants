	abrversionsg = rev-range-this-branch --any -2 versionsg RANGE
	abrtagsg = rev-range-this-branch --any -2 tagsg RANGE
	abrlogchanged = rev-range-this-branch --any -2 logchanged RANGE
	abrlogchangedmine = rev-range-this-branch --any -2 logchangedmine RANGE
	abrlogchangedv = rev-range-this-branch --any -2 logchangedv RANGE
