	bpemaillc = branch-command --keep-position rev-range --revision @{1} --end-revision BRANCH -3 email-command lc RANGE

	;; b2p...: Log variants that cover the differences of the current / passed via -b|--branch branch to where the tip of the branch was second-last.
	b2pd = branch-command --keep-position rev-range --revision @{2} --end-revision BRANCH --with-range ... -2 diff RANGE
	b2pds = branch-command --keep-position files-command --source-command 'b2pfiles --branch BRANCH' rev-range --revision @{2} --end-revision BRANCH --with-range ... -2 diffselected RANGE
	b2pdss = branch-command --keep-position rev-range --revision @{2} --end-revision BRANCH --keep-position selectedcommit-command --single-only --with-range-from-end ^... --range-is-last -3 diff COMMITS RANGE
	b2pdsta = branch-command --keep-position rev-range --revision @{2} --end-revision BRANCH --with-range ... -2 dsta RANGE
