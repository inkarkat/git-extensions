	msgamendrelate-i = loggrange-command -2 amendrelate-i LOGRANGE
	; msgrb is a no-op, because it iterates over the current range without touching fixups.
	msgrbi = loggrange-command -9 selectedcommit-command --single-only --range-is-last -5 previouscommit-command --commit COMMITS rbi LOGRANGE
	msgrbni = loggrange-command -9 selectedcommit-command --single-only --range-is-last -5 previouscommit-command --commit COMMITS rbni LOGRANGE
	msgsegregatecommits = loggrange-command -9 selectedcommit-command --single-only --range-is-last --explicit-file-args -5 previouscommit-command --commit COMMITS segregatecommits LOGRANGE
	msgsegregateandbifurcate = loggrange-command -9 selectedcommit-command --single-only --range-is-last --explicit-file-args -5 previouscommit-command --commit COMMITS segregateandbifurcate LOGRANGE
	msgbifurcate = loggrange-command -9 selectedcommit-command --single-only --range-is-last -5 previouscommit-command --commit COMMITS bifurcate LOGRANGE
	msgrblastfixup = loggrange-command --one-more -2 rblastfixup LOGRANGE
	msgmove-to-branch = loggrange-command --no-range +1 LOGRANGE uncommit-to-branch --exclude-commit
	msguncommit-to-stash = "loggrange-command -9 selectedcommit-command --pass-file-args --range-is-last -5 uncommit-to-branch --commits COMMITS ; LOGRANGE"
	msguncommit-to-branch = loggrange-command -8 selectedcommit-command --single-only --range-is-last -4 uncommit-to-branch --from COMMITS LOGRANGE
	msgcreatebr = loggrange-command --one-more -2 createbrselected LOGRANGE
	msgstackbrfrom = loggrange-command --one-more -2 stackbrfromselected LOGRANGE
	msgdetach = loggrange-command --one-more -2 detachselected LOGRANGE
	msgdetachg = loggrange-command --one-more -2 detachg LOGRANGE
	msgdetachchanged = loggrange-command --one-more -2 detachchanged LOGRANGE
	msgdetachtouched = loggrange-command --one-more -2 detachtouched LOGRANGE
	msgwipe = loggrange-command --one-more -2 wipetoselected LOGRANGE
	msgwipeg = loggrange-command --one-more -2 wipetog LOGRANGE
	msgwipechanged = loggrange-command --one-more -2 wipetochanged LOGRANGE
	msgwipetouched = loggrange-command --one-more -2 wipetotouched LOGRANGE
	msgbase = loggrange-command --no-range -3 name-rev --name-only LOGRANGE
	msgbaselg = loggrange-command --no-range -2 lg1 LOGRANGE
	msgbases = loggrange-command --no-range -2 show LOGRANGE
	msgpred = loggrange-command --no-range --one-more -3 name-rev --name-only LOGRANGE
	msgpredlg = loggrange-command --no-range --one-more -2 lg1 LOGRANGE
	msgpreds = loggrange-command --no-range --one-more -2 show LOGRANGE
	msgcat = loggrange-command -2 catselectedonemore LOGRANGE
	msgcp = loggrange-command -2 cpselectedonemore LOGRANGE
	msgwhocreatedthosechangedfiles = revision-command --keep-position files-command --source-command 'byfiles --revision REVISION' whocreated
	msgwhocreatedhere = loggrange-command --with-log-args -2 whocreated LOGRANGE
	msgwholasttouchedthosechangedfiles = revision-command --keep-position files-command --source-command 'byfiles --revision REVISION' wholasttouched
	msgwholasttouchedhere = loggrange-command --with-log-args -2 wholasttouched LOGRANGE
	msgwhodidthosechangedfiles = revision-command --keep-position files-command --source-command 'byfiles --revision REVISION' whodid
	msgwhodidhere = loggrange-command --with-log-args -2 whodid LOGRANGE
	msgwhodidfhere = loggrange-command --with-log-args -2 whodidf LOGRANGE
	msgwhoownsthosechangedfiles = revision-command --keep-position files-command --source-command 'byfiles --revision REVISION' whoowns
	; msgwhoownsthechanges even with the changed files would not show useful data
	msgwhocontributedthosechangedfiles = revision-command --keep-position files-command --source-command 'byfiles --revision REVISION' whocontributed
	msgwhocontributedhere = loggrange-command --with-log-args -2 whocontributed LOGRANGE
	msgwhowhatthosechangedfiles = revision-command --keep-position files-command --source-command 'byfiles --revision REVISION' whowhat
	msgwhowhathere = loggrange-command --with-log-args -2 whowhat LOGRANGE
	msgwhowhen = loggrange-command --with-log-args -2 whowhen LOGRANGE
	msgwhofirst = loggrange-command --with-log-args -2 whofirst LOGRANGE
	msgwholast = loggrange-command --with-log-args -2 wholast LOGRANGE
	msgwhatdid = loggrange-command --with-log-args -2 whatdid LOGRANGE
	msgchurn = loggrange-command --with-log-args -2 churn LOGRANGE
	msgactivity = loggrange-command --with-log-args -2 activity LOGRANGE
	msgactivitymine = loggrange-command --with-log-args -2 activitymine LOGRANGE
	msgactivityteam = loggrange-command --with-log-args -2 activityteam LOGRANGE
