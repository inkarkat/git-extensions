	pcrbi = rev-range-to-previous-clone --keep-position selectedcommit-command --single-only --range-is-last -5 previouscommit-command --commit COMMITS rbi RANGE
	pcrbni = rev-range-to-previous-clone --keep-position selectedcommit-command --single-only --range-is-last -5 previouscommit-command --commit COMMITS rbni RANGE
	pcbifurcate = rev-range-to-previous-clone --keep-position selectedcommit-command --single-only --range-is-last -5 previouscommit-command --commit COMMITS bifurcate RANGE
	pcrblastfixup = rev-range-to-previous-clone --one-more -2 rblastfixup RANGE
	pcmove-to-branch = rev-range-to-previous-clone --no-range +1 RANGE uncommit-to-branch --exclude-commit
	pcuncommit-to-branch = rev-range-to-previous-clone --keep-position selectedcommit-command --single-only --range-is-last -4 uncommit-to-branch --from COMMITS RANGE
	pccreatebr = rev-range-to-previous-clone -2 createbrselected RANGE
	pcdetach = rev-range-to-previous-clone -2 detachselected RANGE
	pcdetachg = rev-range-to-previous-clone -2 detachselectedg RANGE
	pcwipe = rev-range-to-previous-clone -2 wipetoselected RANGE
	pcwipeg = rev-range-to-previous-clone -2 wipetoselectedg RANGE
	pcbase = rev-range-to-previous-clone --no-range -3 name-rev --name-only RANGE
	pcbaselg = rev-range-to-previous-clone --no-range -2 lg1 RANGE
	pcbases = rev-range-to-previous-clone --no-range -2 show RANGE
	pcpred = rev-range-to-previous-clone --no-range --one-more -3 name-rev --name-only RANGE
	pcpredlg = rev-range-to-previous-clone --no-range --one-more -2 lg1 RANGE
	pcpreds = rev-range-to-previous-clone --no-range --one-more -2 show RANGE
	pccat = rev-range-to-previous-clone -2 catselectedonemore RANGE
	pccp = rev-range-to-previous-clone -2 cpselectedonemore RANGE
	pcwhocreatedthosechangedfiles = branch-command --keep-position files-command --source-command 'pcfiles --branch BRANCH' whocreated
	pcwhocreatedhere = rev-range-to-previous-clone -2 whocreated RANGE
	pcwholasttouchedthosechangedfiles = branch-command --keep-position files-command --source-command 'pcfiles --branch BRANCH' wholasttouched
	pcwholasttouchedhere = rev-range-to-previous-clone -2 wholasttouched RANGE
	pcwhodidthosechangedfiles = branch-command --keep-position files-command --source-command 'pcfiles --branch BRANCH' whodid
	pcwhodidhere = rev-range-to-previous-clone -2 whodid RANGE
	pcwhodidfhere = rev-range-to-previous-clone -2 whodidf RANGE
	pcwhoownsthosechangedfiles = branch-command --keep-position files-command --source-command 'pcfiles --branch BRANCH' whoowns
	; pcwhoownsthechanges even with the changed files would not show useful data
	pcwhocontributedthosechangedfiles = branch-command --keep-position files-command --source-command 'pcfiles --branch BRANCH' whocontributed
	pcwhocontributedhere = rev-range-to-previous-clone -2 whocontributed RANGE
	pcwhowhatthosechangedfiles = branch-command --keep-position files-command --source-command 'pcfiles --branch BRANCH' whowhat
	pcwhowhathere = rev-range-to-previous-clone -2 whowhat RANGE
	pcwhowhen = rev-range-to-previous-clone -2 whowhen RANGE
	pcwhofirst = rev-range-to-previous-clone -2 whofirst RANGE
	pcwholast = rev-range-to-previous-clone -2 wholast RANGE
	pcwhatdid = rev-range-to-previous-clone -2 whatdid RANGE
	pcchurn = rev-range-to-previous-clone -2 churn RANGE
	pcactivity = rev-range-to-previous-clone -2 activity RANGE
	pcactivitymine = rev-range-to-previous-clone -2 activitymine RANGE
	pcactivityteam = rev-range-to-previous-clone -2 activityteam RANGE
	pclogdistribution = rev-range-to-previous-clone -2 logdistribution RANGE
	pclogdistributionmine = rev-range-to-previous-clone -2 logdistributionmine RANGE
	pclogdistributionteam = rev-range-to-previous-clone -2 logdistributionteam RANGE
	pcemaillog = rev-range-to-previous-clone -3 email-command log RANGE
	pcemaillc = rev-range-to-previous-clone -3 email-command lc RANGE

