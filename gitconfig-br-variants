	obmlgby = rbrrefdo-wrapper origin dashdash-default-command --with-files : master-command --keep-position rev-range --revision MASTER --end-revision {} -7 others-command --range RANGE -2 onelinelog AUTHORS RANGE :
	obmlogmine = rbrrefdo-wrapper origin master-command --keep-position rev-range --revision MASTER --end-revision {} -2 logmine RANGE
	obmlogteam = rbrrefdo-wrapper origin master-command --keep-position rev-range --revision MASTER --end-revision {} -2 logteam RANGE
