	; lbmfixup does not make sense with branch references
	; lbmfixuprb does not make sense with branch references
	; lbmcommitidentical does not make sense with branch references
	; lbmcommitlike does not make sense with branch references
	; lbmcommitrelate does not make sense with branch references
	; lbmamendrelate does not make sense with branch references
	; lbmrbi would be a generic implementation of the specialized rebaselocalbr.
	; lbmrbni would be a generic implementation of the specialized rebaselocalbr.
	; lbmwipe does not make sense
	; lbmbase cannot be (easily) passed branch references.
	; lbmbaselg cannot be (easily) passed branch references.
	; lbmpred cannot be (easily) passed branch references.
	; lbmpredlg cannot be (easily) passed branch references.
	lbmwhocreatedthosechangedfiles = localbrrefdo-wrapper --no-git-color files-command --source-command 'brfiles --branch {}' whocreated
	lbmwhocreatedhere = localbrrefdo-wrapper --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -2 whocreated RANGE
	lbmwholasttouchedthosechangedfiles = localbrrefdo-wrapper --no-git-color files-command --source-command 'brfiles --branch {}' wholasttouched
	lbmwholasttouchedhere = localbrrefdo-wrapper --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -2 wholasttouched RANGE
	lbmwhodidthosechangedfiles = localbrrefdo-wrapper --no-git-color files-command --source-command 'brfiles --branch {}' whodid
	lbmwhodidhere = localbrrefdo-wrapper --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -2 whodid RANGE
	lbmwhodidfhere = localbrrefdo-wrapper --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -2 whodidf RANGE
	lbmwhoownsthosechangedfiles = localbrrefdo-wrapper --no-git-color files-command --source-command 'brfiles --branch {}' whoowns
	; lbmwhoownsthechanges even with the changed files would not show useful data
	lbmwhocontributedthosechangedfiles = localbrrefdo-wrapper --no-git-color files-command --source-command 'brfiles --branch {}' whocontributed
	lbmwhocontributedhere = localbrrefdo-wrapper --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -2 whocontributed RANGE
	lbmwhowhatthosechangedfiles = localbrrefdo-wrapper --no-git-color files-command --source-command 'brfiles --branch {}' whowhat
	lbmwhowhathere = localbrrefdo-wrapper --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -2 whowhat RANGE
	lbmwhowhen = localbrrefdo-wrapper --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -2 whowhen RANGE
	lbmwhofirst = localbrrefdo-wrapper --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -2 whofirst RANGE
	lbmwholast = localbrrefdo-wrapper --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -2 wholast RANGE
	lbmwhatdid = localbrrefdo-wrapper --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -2 whatdid RANGE
	lbmchurn = localbrrefdo-wrapper --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -2 churn RANGE
	lbmactivity = localbrrefdo-wrapper --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -2 activity RANGE
	lbmactivitymine = localbrrefdo-wrapper --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -2 activitymine RANGE
	lbmactivityteam = localbrrefdo-wrapper --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -2 activityteam RANGE
	lbmlogdistribution = localbrrefdo-wrapper --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -2 logdistribution RANGE
	lbmlogdistributionmine = localbrrefdo-wrapper --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -2 logdistributionmine RANGE
	lbmlogdistributionteam = localbrrefdo-wrapper --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -2 logdistributionteam RANGE
	lbmemaillog = localbrrefdo-wrapper --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -3 email-command log RANGE
	lbmemaillc = localbrrefdo-wrapper --no-git-color master-command --keep-position rev-range --revision MASTER --end-revision {} -3 email-command lc RANGE

	;; obr...: Log variants that cover all branches in my GitHub clone (like br... ending with a tag or reference anywhere in the history (also by remotes))
	obrd = rbrrefdo-wrapper origin rev-range-this-branch --branch {} --any --with-range ... -2 diff RANGE
	obrds = "!sh -c \"GIT_SELECTED_COMMAND_DEFAULT_FILES='git obrfiles --no-header | sort --unique' git-selected-command obrd\""
