[alias]
	coom = master-command -2 checkout origin/MASTER	; Checkout [<path>s from] my GitHub clone's master branch.
	omco = master-command +1 origin/MASTER checkoutbranch ; Use with -b <newbranch> to start a patch based on my GitHub clone's master branch (if I mistakenly committed to master but now want a branch).
	omcosuperbr = master-command +1 origin/MASTER cosuperbr
	omcosub = omco --submodules	; Checkout local master branch from origin, also checkout eponymous remote master branches in submodules.
	omdetach = master-command -2 detach origin/MASTER
	omreset = master-command +1 origin/MASTER reset	; Reset my current branch to the HEAD of the master branch of my GitHub clone.
	omhardreset = omreset --hard ; Reset my current branch to the HEAD of the master branch of my GitHub clone.
	omwipe = master-command -2 wipe origin/MASTER
	omd = master-command -2 diff ..origin/MASTER ; Show differences between my GitHub clone's master branch and HEAD.
	omdfiles = master-command -3 diff --name-only ..origin/MASTER ; Show changed files between my GitHub clone's master branch and HEAD.
	omdsta = master-command -2 dsta ..origin/MASTER
	omdstat = master-command -2 dstat ..origin/MASTER
	omdl = master-command -2 dl ..origin/MASTER
	omdt = master-command -2 difftool ..origin/MASTER
	omdr = master-command -2 dr ..origin/MASTER
	omdrl = master-command -2 drl ..origin/MASTER
	oml = master-command -2 l origin/MASTER
	omlogmod = master-command -2 logmod origin/MASTER
	omlogadded = master-command -2 logadded origin/MASTER
	omlogdeleted = master-command -2 logdeleted origin/MASTER
	omlogrenamed = master-command -2 logrenamed origin/MASTER
	omlogmodfiles = master-command -2 logmodfiles origin/MASTER
	omlogaddedfiles = master-command -2 logaddedfiles origin/MASTER
	omlogdeletedfiles = master-command -2 logdeletedfiles origin/MASTER
	omlogrenamedfiles = master-command -2 logrenamedfiles origin/MASTER
	omrevive = master-command -3 revive --all origin/MASTER
	omlc = master-command -2 lc origin/MASTER
	omlg = master-command -2 lg origin/MASTER
	omlghi = master-command -3 lghi origin/MASTER
	omlghist = master-command -2 lghist origin/MASTER
	omlghii = master-command -2 lghii origin/MASTER
	omlghi-i = master-command -2 lghi-i origin/MASTER
	omlgmine = master-command -2 lgmine origin/MASTER
	omlog = master-command -2 log origin/MASTER
	omlogf = master-command -2 logf origin/MASTER
	omlogv = master-command -2 logv origin/MASTER
	omlogfiles = master-command -2 logfiles origin/MASTER
	omshowfilesm = master-command -2 showfiles MASTER..origin/MASTER
	ominout = master-command -3 inout --base origin/MASTER
	omiofiles = master-command -4 inout --files --base origin/MASTER
	omiosubmodules = master-command -4 inout --submodules --base origin/MASTER
	omio = master-command -3 io --base origin/MASTER
	omab = master-command -3 ab --base origin/MASTER
	omcat = master-command -2 cat origin/MASTER
	omcp = master-command -2 cp origin/MASTER
	omrb = master-command +1 origin/MASTER rebase ; Rebase my commits on top of the latest master of my GitHub clone.
	omrbi = master-command +1 origin/MASTER rbi
	omrbni = master-command +1 origin/MASTER rbni
	ommerge = master-command +1 origin/MASTER merge ; Merge changes done to the master branch of my GitHub clone with my local repo.
	ommergedr = master-command +1 origin/MASTER mergedryrun ; Test merge of changes done to the master branch of my GitHub clone with my local repo.

	omcom = master-command +1 origin/MASTER checkout-remote-branch	; Checkout and reset master to my GitHub clone's master branch.
	omdm = master-command -2 diff MASTER..origin/MASTER ; Show differences introduced in my GitHub clone relative to master.
	omdfilesm = master-command -3 diff --name-only MASTER..origin/MASTER ; Show changed files introduced in my GitHub clone relative to master.
	omdstam = master-command -2 dsta MASTER..origin/MASTER
	omdstatm = master-command -2 dstat MASTER..origin/MASTER
	omdlm = master-command -2 dl MASTER..origin/MASTER
	omdtm = master-command -2 difftool MASTER..origin/MASTER
	omdrm = master-command -2 dr MASTER..origin/MASTER
	omdrlm = master-command -2 drl MASTER..origin/MASTER
	omlm = master-command -2 l MASTER..origin/MASTER
	omlmogmod = master-command -2 logmod MASTER..origin/MASTER
	omlmogadded = master-command -2 logadded MASTER..origin/MASTER
	omlmogdeleted = master-command -2 logdeleted MASTER..origin/MASTER
	omlmogrenamed = master-command -2 logrenamed MASTER..origin/MASTER
	omlmogmodfiles = master-command -2 logmodfiles MASTER..origin/MASTER
	omlmogaddedfiles = master-command -2 logaddedfiles MASTER..origin/MASTER
	omlmogdeletedfiles = master-command -2 logdeletedfiles MASTER..origin/MASTER
	omlmogrenamedfiles = master-command -2 logrenamedfiles MASTER..origin/MASTER
	omlrevive = master-command -3 revive --all MASTER..origin/MASTER
	omlcm = master-command --keep-position rev-range --revision MASTER --end-revision origin/MASTER --one-more-command log --one-more-with-padding -2 lc RANGE
	omlgm = master-command --keep-position rev-range --revision MASTER --end-revision origin/MASTER --one-more-command greyonelinelog --one-more-only-to-terminal -2 lg RANGE
	omlghim = master-command -3 lghi MASTER..origin/MASTER
	omlghistm = master-command -2 lghist MASTER..origin/MASTER
	omlghiim = master-command -2 lghii MASTER..origin/MASTER
	omlghi-im = master-command -2 lghi-i MASTER..origin/MASTER
	omlgminem = master-command -2 lgmine MASTER..origin/MASTER
	omlogm = master-command --keep-position rev-range --revision MASTER --end-revision origin/MASTER --one-more-command greylog --one-more-with-padding --one-more-only-to-terminal -2 log RANGE
	omlogfm = master-command --keep-position rev-range --revision MASTER --end-revision origin/MASTER --one-more-command greylog --one-more-with-padding --one-more-only-to-terminal -2 logf RANGE
	omlogvm = master-command --keep-position rev-range --revision MASTER --end-revision origin/MASTER --one-more-command greylog --one-more-with-padding --one-more-only-to-terminal -2 logv RANGE
	omlogfilesm = master-command -2 logfiles MASTER..origin/MASTER
	omsubmodulesm = master-command -2 showsubmodules MASTER..origin/MASTER
	ominoutm = master-command +3 --base MASTER origin/MASTER inout
	omiofilesm = master-command +3 --base MASTER origin/MASTER inout --files
	omiosubmodulesm = master-command +3 --base MASTER origin/MASTER inout --submodules
	omiom = master-command +3 --base MASTER origin/MASTER io
	omabm = master-command +3 --base MASTER origin/MASTER ab

	; omcoh does not make sense; cannot reset HEAD, only a branch.
	omdh = master-command -2 diff HEAD..origin/MASTER ; Show differences introduced in my GitHub clone relative to HEAD.
	omdfilesh = master-command -3 diff --name-only HEAD..origin/MASTER ; Show changed files introduced in my GitHub clone relative to HEAD.
	omdstah = master-command -2 dsta HEAD..origin/MASTER
	omdstath = master-command -2 dstat HEAD..origin/MASTER
	omdlh = master-command -2 dl HEAD..origin/MASTER
	omdth = master-command -2 difftool HEAD..origin/MASTER
	omdrh = master-command -2 dr HEAD..origin/MASTER
	omdrlh = master-command -2 drl HEAD..origin/MASTER
	omlh = master-command -2 l HEAD..origin/MASTER
	omlhogmod = master-command -2 logmod HEAD..origin/MASTER
	omlhogadded = master-command -2 logadded HEAD..origin/MASTER
	omlhogdeleted = master-command -2 logdeleted HEAD..origin/MASTER
	omlhogrenamed = master-command -2 logrenamed HEAD..origin/MASTER
	omlhogmodfiles = master-command -2 logmodfiles HEAD..origin/MASTER
	omlhogaddedfiles = master-command -2 logaddedfiles HEAD..origin/MASTER
	omlhogdeletedfiles = master-command -2 logdeletedfiles HEAD..origin/MASTER
	omlhogrenamedfiles = master-command -2 logrenamedfiles HEAD..origin/MASTER
	omlrevive = master-command -3 revive --all HEAD..origin/MASTER
	omlch = master-command --keep-position rev-range --revision HEAD --end-revision origin/MASTER --one-more-command log --one-more-with-padding -2 lc RANGE
	omlgh = master-command --keep-position rev-range --revision HEAD --end-revision origin/MASTER --one-more-command greyonelinelog --one-more-only-to-terminal -2 lg RANGE
	omlghih = master-command -3 lghi HEAD..origin/MASTER
	omlghisth = master-command -2 lghist HEAD..origin/MASTER
	omlghiih = master-command -2 lghii HEAD..origin/MASTER
	omlghi-ih = master-command -2 lghi-i HEAD..origin/MASTER
	omlgmineh = master-command -2 lgmine HEAD..origin/MASTER
	omlogh = master-command --keep-position rev-range --revision HEAD --end-revision origin/MASTER --one-more-command greylog --one-more-with-padding --one-more-only-to-terminal -2 log RANGE
	omlogfh = master-command --keep-position rev-range --revision HEAD --end-revision origin/MASTER --one-more-command greylog --one-more-with-padding --one-more-only-to-terminal -2 logf RANGE
	omlogvh = master-command --keep-position rev-range --revision HEAD --end-revision origin/MASTER --one-more-command greylog --one-more-with-padding --one-more-only-to-terminal -2 logv RANGE
	omlogfilesh = master-command -2 logfiles HEAD..origin/MASTER
	omsubmodulesh = master-command -2 showsubmodules HEAD..origin/MASTER
	ominouth = master-command +3 --base HEAD origin/MASTER inout
	omiofilesh = master-command +3 --base HEAD origin/MASTER inout --files
	omiosubmodulesh = master-command +3 --base HEAD origin/MASTER inout --submodules
	omioh = master-command +3 --base HEAD origin/MASTER io
	omabh = master-command +3 --base HEAD origin/MASTER ab

	coon = base-command -2 checkout origin/BASE	; Checkout [<path>s from] my GitHub clone's base branch.
	onco = base-command +1 origin/BASE checkoutbranch ; Use with -b <newbranch> to start a patch based on my GitHub clone's base branch (if I mistakenly committed to master but now want a branch).
	oncosuperbr = base-command +1 origin/BASE cosuperbr
	oncosub = onco --submodules	; Checkout local base branch from origin, also checkout eponymous remote base branches in submodules.
	ondetach = base-command -2 detach origin/BASE
	onreset = base-command +1 origin/BASE reset	; Reset my current branch to the HEAD of the base branch of origin.
	onhardreset = onreset --hard ; Reset my current branch to the HEAD of the base branch of my GitHub clone.
	onwipe = base-command -2 wipe origin/BASE
	ond = base-command -2 diff ..origin/BASE ; Show differences between my GitHub clone's base branch and HEAD.
	ondfiles = base-command -3 diff --name-only ..origin/BASE ; Show changed files between my GitHub clone's base branch and HEAD.
	ondsta = base-command -2 dsta ..origin/BASE
	ondstat = base-command -2 dstat ..origin/BASE
	ondl = base-command -2 dl ..origin/BASE
	ondt = base-command -2 difftool ..origin/BASE
	ondr = base-command -2 dr ..origin/BASE
	ondrl = base-command -2 drl ..origin/BASE
	onl = base-command -2 l origin/BASE
	onlogmod = base-command -2 logmod origin/BASE
	onlogadded = base-command -2 logadded origin/BASE
	onlogdeleted = base-command -2 logdeleted origin/BASE
	onlogrenamed = base-command -2 logrenamed origin/BASE
	onlogmodfiles = base-command -2 logmodfiles origin/BASE
	onlogaddedfiles = base-command -2 logaddedfiles origin/BASE
	onlogdeletedfiles = base-command -2 logdeletedfiles origin/BASE
	onlogrenamedfiles = base-command -2 logrenamedfiles origin/BASE
	onrevive = base-command -3 revive --all origin/BASE
	onlc = base-command -2 lc origin/BASE
	onlg = base-command -2 lg origin/BASE
	onlghi = base-command -3 lghi origin/BASE
	onlghist = base-command -2 lghist origin/BASE
	onlghii = base-command -2 lghii origin/BASE
	onlghi-i = base-command -2 lghi-i origin/BASE
	onlgmine = base-command -2 lgmine origin/BASE
	onlog = base-command -2 log origin/BASE
	onlogf = base-command -2 logf origin/BASE
	onlogv = base-command -2 logv origin/BASE
	onlogfiles = base-command -2 logfiles origin/BASE
	onshowfiles = base-command -2 showfiles origin/BASE
	oninout = base-command -3 inout --base origin/BASE
	oniofiles = base-command -4 inout --files --base origin/BASE
	oniosubmodules = base-command -4 inout --submodules --base origin/BASE
	onio = base-command -3 io --base origin/BASE
	onab = base-command -3 ab --base origin/BASE
	oncat = base-command -2 cat origin/BASE
	oncp = base-command -2 cp origin/BASE
	onrb = base-command +1 origin/BASE rebase ; Rebase my commits on top of the latest base branch of my GitHub clone.
	onrbi = base-command +1 origin/BASE rbi
	onrbni = base-command +1 origin/BASE rbni
	onmerge = base-command +1 origin/BASE merge ; Merge changes done to the base branch of my GitHub clone with my local repo.
	onmergedr = base-command +1 origin/BASE mergedryrun ; Test merge of changes done to the base branch of my GitHub clone with my local repo.

	oncon = base-command +1 origin/BASE checkout-remote-branch	; Checkout and reset master to my GitHub clone's base branch.
	ondn = base-command -2 diff BASE..origin/BASE ; Show differences introduced in my GitHub clone relative to base branch.
	ondfilesn = base-command -3 diff --name-only BASE..origin/BASE ; Show changed files introduced in my GitHub clone relative to base branch.
	ondstan = base-command -2 dsta BASE..origin/BASE
	ondstatn = base-command -2 dstat BASE..origin/BASE
	ondln = base-command -2 dl BASE..origin/BASE
	ondtn = base-command -2 difftool BASE..origin/BASE
	ondrn = base-command -2 dr BASE..origin/BASE
	ondrln = base-command -2 drl BASE..origin/BASE
	onln = base-command -2 l BASE..origin/BASE
	onlnogmod = base-command -2 logmod BASE..origin/BASE
	onlnogadded = base-command -2 logadded BASE..origin/BASE
	onlnogdeleted = base-command -2 logdeleted BASE..origin/BASE
	onlnogrenamed = base-command -2 logrenamed BASE..origin/BASE
	onlnogmodfiles = base-command -2 logmodfiles BASE..origin/BASE
	onlnogaddedfiles = base-command -2 logaddedfiles BASE..origin/BASE
	onlnogdeletedfiles = base-command -2 logdeletedfiles BASE..origin/BASE
	onlnogrenamedfiles = base-command -2 logrenamedfiles BASE..origin/BASE
	onlrevive = base-command -3 revive --all BASE..origin/BASE
	onlcn = base-command --keep-position rev-range --revision BASE --end-revision origin/BASE --one-more-command log --one-more-with-padding -2 lc RANGE
	onlgn = base-command --keep-position rev-range --revision BASE --end-revision origin/BASE --one-more-command greyonelinelog --one-more-only-to-terminal -2 lg RANGE
	onlghin = base-command -3 lghi BASE..origin/BASE
	onlghistn = base-command -2 lghist BASE..origin/BASE
	onlghiin = base-command -2 lghii BASE..origin/BASE
	onlghi-in = base-command -2 lghi-i BASE..origin/BASE
	onlgminen = base-command -2 lgmine BASE..origin/BASE
	onlogn = base-command --keep-position rev-range --revision BASE --end-revision origin/BASE --one-more-command greylog --one-more-with-padding --one-more-only-to-terminal -2 log RANGE
	onlogfn = base-command --keep-position rev-range --revision BASE --end-revision origin/BASE --one-more-command greylog --one-more-with-padding --one-more-only-to-terminal -2 logf RANGE
	onlogvn = base-command --keep-position rev-range --revision BASE --end-revision origin/BASE --one-more-command greylog --one-more-with-padding --one-more-only-to-terminal -2 logv RANGE
