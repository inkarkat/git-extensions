	onsubmodulesn = base-command -2 showsubmodules BASE..origin/BASE
	oninoutn = base-command +3 --base BASE origin/BASE inout
	oniofilesn = base-command +3 --base BASE origin/BASE inout --files
	oniosubmodulesn = base-command +3 --base BASE origin/BASE inout --submodules
	onion = base-command +3 --base BASE origin/BASE io
	onabn = base-command +3 --base BASE origin/BASE ab

	; oncoh does not make sense; cannot reset HEAD, only a branch.
	ondh = base-command -2 diff HEAD..origin/BASE ; Show differences introduced in my GitHub clone relative to HEAD.
	ondfilesh = base-command -3 diff --name-only HEAD..origin/BASE ; Show changed files introduced in my GitHub clone relative to HEAD.
	ondstah = base-command -2 dsta HEAD..origin/BASE
	ondstath = base-command -2 dstat HEAD..origin/BASE
	ondlh = base-command -2 dl HEAD..origin/BASE
	ondth = base-command -2 difftool HEAD..origin/BASE
	ondrh = base-command -2 dr HEAD..origin/BASE
	ondrlh = base-command -2 drl HEAD..origin/BASE
